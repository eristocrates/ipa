fibo-fndna-all: dcterms:abstract "The North American extension of the Foundations (FND) domain builds on the more general FND ontologies with concepts that are specific to the North American region of the world, such as those defining North American addresses.";
                dcterms:contributor "Adaptive, Inc.",
                                    "Anthony B. Coates",
                                    "Hypercube Limited",
                                    "John F. Gemski",
                                    "Maxwell Gillmore",
                                    "Nordea Bank AB",
                                    "Office of Financial Research (US Dept of the Treasury)",
                                    "Pinnacle Bank (Morgan Hill, California)",
                                    "State Street Bank and Trust",
                                    "Tahoe Blue Ltd",
                                    "Thematix Partners LLC",
                                    "Wells Fargo",
                                    "Working Ontologist",
                                    "agnos.ai, UK Ltd.";
                dcterms:issued "2020-03-22T18:00:00"^^xsd:dateTime;
                dcterms:license "https://opensource.org/licenses/MIT"^^xsd:anyURI;
                dcterms:modified "2023-01-20T18:00:00"^^xsd:dateTime;
                dcterms:title "FIBO FND Domain, North American Extension",
                              "Financial Industry Business Ontology (FIBO) Foundations (FND) Domain, North American Extension";
                a owl:Ontology;
                rdfs:label "all FND, North America";
                rdfs:seeAlso "https://spec.edmcouncil.org/fibo/"^^xsd:anyURI;
                owl:imports <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/AllFND/>,
                            <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Places/NorthAmerica/USPostalServiceAddresses/>,
                            <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Places/NorthAmerica/USPostalServiceAddressesIndividuals/>;
                owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/AllFND-NorthAmerica/>;
