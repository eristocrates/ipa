                 a owl:Ontology;
                 rdfs:label "Real Property Ontology"@en;
                 owl:imports <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Accounting/AccountingEquity/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/Assessments/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/DatesAndTimes/Occurrences/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/OwnershipAndControl/Ownership/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Places/Addresses/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Relations/Relations/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/AnnotationVocabulary/>,
                             cmns-av:,
                             cmns-col:,
                             cmns-cxtdsg:,
                             cmns-cxtid:,
                             cmns-doc:,
                             cmns-id:,
                             <https://www.omg.org/spec/Commons/Locations/>,
