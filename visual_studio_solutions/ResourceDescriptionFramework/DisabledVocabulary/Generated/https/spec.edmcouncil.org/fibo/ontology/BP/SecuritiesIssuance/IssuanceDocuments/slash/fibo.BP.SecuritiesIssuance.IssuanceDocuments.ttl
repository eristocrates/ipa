                 a owl:Ontology;
                 rdfs:label "IssuanceDocuments"@en;
                 owl:imports <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Agreements/Contracts/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Law/LegalCore/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/AnnotationVocabulary/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/SEC/Securities/SecuritiesIssuance/>,
                             cmns-av:,
                             cmns-dt:;
                 owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/latest/BP/SecuritiesIssuance/IssuanceDocuments/>;
                 fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Provisional;
                 cmns-av:copyright "Copyright (c) 2013-2025 EDM Council, Inc.".
fibo-bp-iss-doc:FinalProspectus a owl:Class;
                                rdfs:isDefinedBy fibo-bp-iss-doc:;
                                rdfs:label "final prospectus"@en;
                                rdfs:subClassOf fibo-sec-sec-iss:Prospectus,
                                                [a owl:Restriction ; 
                                                 owl:onProperty fibo-fnd-agr-ctr:hasContractualElement ; 
                                                 owl:someValuesFrom fibo-bp-iss-doc:OfferingDocumentTerms],
                                                [a owl:Restriction ; 
                                                 owl:onProperty cmns-dt:succeeds ; 
                                                 owl:someValuesFrom fibo-bp-iss-doc:PreliminaryProspectus];
                                skos:definition "The final Prospectus for an issue describes the Offer Issue, including facts about the issue itself such as closing dates, and known facts about the securities that will form part of that issue."@en;
                                cmns-av:explanatoryNote "Anticipated facts might include expected issue price, yields, pool parameters as appropriate for the type of instrument being issued. Term origin:SMER"@en.
fibo-bp-iss-doc:Indenture a owl:Class;
                          rdfs:isDefinedBy fibo-bp-iss-doc:;
                          rdfs:label "indenture"@en;
                          rdfs:subClassOf fibo-bp-iss-doc:SecuritiesIssuanceAgreement,
                                          fibo-fnd-agr-ctr:WrittenContract;
                          skos:definition """A written contract, also known as a \"Deed of Trust\", under which bonds and debentures are issued, setting forth maturity date, interest rate, redemption rights, call privileges and other terms. Under the rules of the Trust Indenture Act of 1939, the contract is executed by the issuer and a trustee who acts on behalf of the bondholders."""@en.
fibo-bp-iss-doc:IssuanceTrustAgreement a owl:Class;
                                       rdfs:isDefinedBy fibo-bp-iss-doc:;
                                       rdfs:label "issuance trust agreement"@en;
                                       rdfs:subClassOf fibo-bp-iss-doc:SecuritiesIssuanceAgreement,
                                                       fibo-fnd-agr-ctr:WrittenContract.
fibo-bp-iss-doc:OfferingDocumentTerms a owl:Class;
                                      rdfs:isDefinedBy fibo-bp-iss-doc:;
                                      rdfs:label "offering document terms"@en;
                                      rdfs:subClassOf fibo-fnd-agr-ctr:ContractualElement;
                                      skos:definition "terms included in an offering document that become legally binding at issue"@en;
                                      cmns-av:explanatoryNote "They include, for example, call terms, interest payment terms, and so forth with respect to certain securities."@en.
fibo-bp-iss-doc:PreliminaryProspectus a owl:Class;
                                      rdfs:isDefinedBy fibo-bp-iss-doc:;
                                      rdfs:label "preliminary prospectus"@en;
                                      rdfs:subClassOf fibo-sec-sec-iss:Prospectus,
                                                      [a owl:Restriction ; 
                                                       owl:onProperty fibo-fnd-agr-ctr:hasContractualElement ; 
                                                       owl:someValuesFrom fibo-bp-iss-doc:OfferingDocumentTerms];
                                      skos:definition "The draft prospectus for the issue, as determined prior to marketing the issue. Certain terms in the draft prospectus will be finalized later in the issuance process to become the actual Prospectus. Term origin:DTCC issuance Reviews"@en.
fibo-bp-iss-doc:SecuritiesIssuanceAgreement a owl:Class;
                                            rdfs:isDefinedBy fibo-bp-iss-doc:;
                                            rdfs:label "securities issuance agreement"@en;
                                            rdfs:subClassOf fibo-sec-sec-iss:OfferingDocument;
                                            skos:definition "A formal agreement, deed or contract in respect of some intended securities issuance."@en.
fibo-sec-sec-iss:OfferingDocument a owl:Class;
                                  rdfs:subClassOf [a owl:Restriction ; 
                                                   owl:onProperty fibo-fnd-agr-ctr:hasContractualElement ; 
                                                   owl:someValuesFrom fibo-bp-iss-doc:OfferingDocumentTerms].
