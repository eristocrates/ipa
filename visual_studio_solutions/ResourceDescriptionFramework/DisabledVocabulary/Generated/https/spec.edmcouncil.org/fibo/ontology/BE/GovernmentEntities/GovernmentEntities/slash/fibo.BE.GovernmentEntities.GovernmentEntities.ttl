@prefix skos: <http://www.w3.org/2004/02/skos/core#>.

_:autos83 rdf:first [a owl:Restriction ; 
                     owl:onProperty cmns-rga:governs ; 
                     owl:onClass cmns-loc:FederalCapitalArea ; 
                     owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger];
          rdf:rest _:autos84.
_:autos84 rdf:first [a owl:Restriction ; 
                     owl:onProperty cmns-rga:governs ; 
                     owl:someValuesFrom cmns-loc:FederalState];
          rdf:rest rdf:nil.
_:autos88 rdf:first [a owl:Restriction ; 
                     owl:onProperty fibo-be-ge-ge:hasFullSovereigntyOver ; 
                     owl:someValuesFrom cmns-loc:Country];
          rdf:rest _:autos89.
_:autos89 rdf:first [a owl:Restriction ; 
                     owl:onProperty fibo-be-ge-ge:hasSharedSovereigntyOver ; 
                     owl:someValuesFrom cmns-loc:FederalState];
          rdf:rest rdf:nil.
_:autos93 rdf:first [a owl:Restriction ; 
                     owl:onProperty fibo-be-ge-ge:isRepresentedBy ; 
                     owl:someValuesFrom fibo-be-ge-ge:RegionalGovernment];
          rdf:rest _:autos94.
_:autos94 rdf:first [a owl:Restriction ; 
                     owl:onProperty fibo-be-ge-ge:isRepresentedBy ; 
                     owl:onClass fibo-be-ge-ge:FederalGovernment ; 
                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger];
          rdf:rest rdf:nil.
_:autos97 rdf:first fibo-be-ge-ge:BranchOfGovernment;
          rdf:rest _:autos98.
_:autos98 rdf:first fibo-be-ge-ge:GovernmentAgency;
          rdf:rest _:autos99.
_:autos99 rdf:first fibo-be-ge-ge:GovernmentDepartment;
          rdf:rest rdf:nil.
fibo-be-ge-ge: dcterms:abstract "This ontology defines the fundamental concepts for representing polities and government entities and their relations.";
