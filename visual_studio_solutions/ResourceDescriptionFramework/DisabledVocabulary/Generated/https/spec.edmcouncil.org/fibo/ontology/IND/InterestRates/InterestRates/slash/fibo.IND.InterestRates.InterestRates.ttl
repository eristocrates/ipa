                           cmns-dt:hasDurationValue "P60M".
fibo-ind-ir-ir:SpecificProviderInterestRateBenchmark a owl:Class;
                                                     rdfs:isDefinedBy fibo-ind-ir-ir:;
                                                     rdfs:label "specific-provider interest rate benchmark";
                                                     rdfs:subClassOf fibo-ind-ir-ir:InterestRateBenchmark,
                                                                     [a owl:Restriction ; 
