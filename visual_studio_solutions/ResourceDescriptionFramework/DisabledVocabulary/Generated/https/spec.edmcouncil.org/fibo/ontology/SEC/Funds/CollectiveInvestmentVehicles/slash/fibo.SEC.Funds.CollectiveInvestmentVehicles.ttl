                                             skos:definition "Formal terms for general processing of the fund. These set out what the investor and the fund may or may not do."@en.
fibo-sec-fund-civ:FundProcessingTerms a owl:Class;
                                      rdfs:isDefinedBy fibo-sec-fund-civ:;
                                      rdfs:label "fund processing terms"@en;
                                      rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment,
                                                      [a owl:Restriction ; 
                                                       owl:onProperty fibo-sec-fund-civ:definesMainFundOrderDeskAccount ; 
                                                       owl:someValuesFrom fibo-sec-fund-civ:FundsProcessingAccount],
