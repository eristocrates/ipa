fibo-der-drc-bsc:CalculationAgent a owl:Class;
                                  rdfs:isDefinedBy fibo-der-drc-bsc:;
                                  owl:deprecated true;
                                  owl:equivalentClass fibo-fbc-fi-fi:CalculationAgent.
fibo-der-drc-bsc:CashflowExpression a owl:Class;
                                    rdfs:isDefinedBy fibo-der-drc-bsc:;
                                    rdfs:label "cashflow expression";
                                    rdfs:subClassOf cmns-qtu:Expression;
                                    skos:definition "expression that specifies a calculation of a cash flow as a component of a cashflow formula".
fibo-der-drc-bsc:CashflowFormula a owl:Class;
                                 rdfs:isDefinedBy fibo-der-drc-bsc:;
                                 rdfs:label "cashflow formula";
                                 rdfs:subClassOf fibo-fnd-utl-alx:Formula,
                                                 [a owl:Restriction ; 
                                                  owl:onProperty cmns-qtu:hasExpression ; 
                                                  owl:someValuesFrom fibo-der-drc-bsc:CashflowExpression];
                                 skos:definition "formula for determining cashflows for a derivative instrument".
fibo-der-drc-bsc:CashflowTerms a owl:Class;
                               rdfs:isDefinedBy fibo-der-drc-bsc:;
                               rdfs:label "cashflow terms";
                               rdfs:subClassOf fibo-der-drc-bsc:DerivativeTerms,
                                               fibo-fbc-dae-dbt:DebtTerms;
                               skos:definition "terms setting out a cashflow structure of payments committed to by one party to a contract";
                               skos:editorialNote "Swap cashflows are known as Swapstreams and are the terms for payment to and from either party. These are defined in swap transaction messages and represent the terms of the contract implied by that transaction. Options (Nordea reviews): Cashflows are defined as Payouts. This is not the same as a model of a cashflow which is a consequence of applying some legal term for payment of interest or principal, but is a commitment expressed in purely cashflow terms. Review this though. Payout terms include: Values - values can only go up or down; Static values are defined for limits and the like. i.e. Constraints (and direction) - this covers caps and floors - these are read upward or downward Conditionality Formula relations (Input and Output): - these are values - these may have a cap or a floor on them also - these also may have Multiplication - there is also fixed margin - may have addition or substraction between these Linearity in covered in the above Timing / expiry Observaton (not terms): Probability Sensitivity".
fibo-der-drc-bsc:ContractForDifference a owl:Class;
                                       rdfs:isDefinedBy fibo-der-drc-bsc:;
                                       rdfs:label "contract for difference"@en;
                                       rdfs:subClassOf fibo-fbc-fi-fi:DerivativeInstrument;
                                       skos:definition "cash-settled derivative where the parties agree to exchange on the maturity of the contract the difference between the current value of the underlying asset and the initial value of that asset when the contract is initiated"@en;
                                       skos:example "For example, suppose the initial price of share XYZ is $100 and a CFD for 1000 shares is exchanged. Both the buyer and seller must post some margin. If the price goes to $105, then the buyer gets $5,000 from the seller. If the price goes to $95, the buyer pays the seller $5,000. This contract avoids ownership of the stock and all the associated transactions issues (like stamp taxes). The contract also allows for leverage (typically 10:1) because the margin that must be posted is only a fraction of the value of the underlying asset."@en;
                                       cmns-av:abbreviation "CFD"@en;
                                       cmns-av:adaptedFrom "ISO 10962:2019, Securities and related financial instruments - Classification of financial instruments (CFI) code",
                                                           "https://www.nasdaq.com/glossary/c/contract-for-difference"^^xsd:anyURI;
                                       cmns-av:explanatoryNote "These contracts can also be on the difference of two assets' prices. They can also be on the difference of a single asset of different maturities (like a bond or futures contracts)."@en;
                                       cmns-av:synonym "spread trading"@en.
fibo-der-drc-bsc:CreditDerivative a owl:Class;
                                  rdfs:isDefinedBy fibo-der-drc-bsc:;
                                  rdfs:label "credit derivative"@en;
                                  rdfs:subClassOf fibo-fbc-fi-fi:DerivativeInstrument;
                                  skos:definition "derivative instrument that is a privately held, negotiable bilateral contract traded over-the-counter (OTC) between two parties in a creditor/debtor relationship, enabling the creditor to effectively transfer some or all of the risk of a debtor defaulting to a third party"@en;
                                  skos:example "Examples include credit default swaps (CDS), collateralized debt obligations (CDO), total return swaps, and credit spread options and forwards."@en;
                                  cmns-av:explanatoryNote "The third party accepts the risk in return for payment, known as the premium."@en.
fibo-der-drc-bsc:DerivativeSettlementTerms a owl:Class;
                                           rdfs:isDefinedBy fibo-der-drc-bsc:;
                                           rdfs:label "derivative settlement terms";
                                           rdfs:subClassOf fibo-der-drc-bsc:DerivativeTerms,
                                                           fibo-fbc-pas-fpas:SettlementTerms,
                                                           [a owl:Restriction ; 
                                                            owl:onProperty fibo-der-drc-bsc:hasAdditionalCosts ; 
                                                            owl:onClass fibo-fnd-acc-cur:MonetaryAmount ; 
                                                            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                                           [a owl:Restriction ; 
                                                            owl:onProperty fibo-der-drc-bsc:hasUnderlyingAssetPrice ; 
                                                            owl:onClass fibo-fnd-acc-cur:MonetaryPrice ; 
                                                            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger];
                                           skos:definition "settlement terms specifying additional details with respect to what is to be delivered when, to whom, under what conditions at the time of settlement".
fibo-der-drc-bsc:DerivativeTerms a owl:Class;
                                 rdfs:isDefinedBy fibo-der-drc-bsc:;
                                 rdfs:label "derivative terms";
                                 rdfs:subClassOf fibo-fnd-agr-ctr:ContractualCommitment;
                                 skos:definition "contractual terms specific to derivative contracts, including terms related to payments and delivery between parties".
fibo-der-drc-bsc:DerivativesClearingOrganization a owl:Class;
                                                 rdfs:isDefinedBy fibo-der-drc-bsc:;
                                                 rdfs:label "derivatives clearing organization";
                                                 rdfs:subClassOf fibo-fbc-fct-fse:ClearingHouse;
                                                 skos:definition "clearing house that enables parties to substitute the credit of the DCO for the credit of the parties";
                                                 cmns-av:abbreviation "DCO";
                                                 cmns-av:adaptedFrom "http://www.cftc.gov/IndustryOversight/ClearingOrganizations/index.htm"^^xsd:anyURI;
                                                 cmns-av:explanatoryNote "Substitution may be done through contract novation, for example. A derivatives clearing organization (DCO) also arranges or provides, on a multilateral basis, for the settlement or netting of obligations, or otherwise provides clearing services or arrangements that mutualize or transfer credit risk among participants.".
fibo-der-drc-bsc:EconomicRateBasedDerivative a owl:Class;
                                             rdfs:isDefinedBy fibo-der-drc-bsc:;
                                             rdfs:label "economic rate-based derivative";
                                             rdfs:subClassOf fibo-der-drc-bsc:RateBasedDerivative,
                                                             [a owl:Restriction ; 
                                                              owl:onProperty fibo-fbc-fi-fi:hasUnderlier ; 
                                                              owl:someValuesFrom [a owl:Restriction ; 
                                                                                  owl:onProperty cmns-rlcmp:isPlayedBy ; 
                                                                                  owl:someValuesFrom fibo-ind-ei-ei:EconomicIndicator]];
                                             skos:definition "rate-based derivative whose underlier is some economic indicator".
fibo-der-drc-bsc:ForwardRateAgreement a owl:Class;
                                      rdfs:isDefinedBy fibo-der-drc-bsc:;
                                      rdfs:label "forward rate agreement"@en;
                                      rdfs:subClassOf fibo-der-drc-bsc:InterestRateDerivative;
                                      skos:definition "agreement to exchange an interest rate commitment on a notional amount at a future date"@en;
                                      cmns-av:abbreviation "FRA"@en;
                                      cmns-av:explanatoryNote "The FRA determines the rates to be used along with the termination date and notional value. FRAs are cash-settled with the payment based on the net difference between the interest rate of the contract and the floating rate in the market called the reference rate. The notional amount is not exchanged, but rather a cash amount based on the rate differentials and the notional value of the contract."@en.
fibo-der-drc-bsc:InterestRateDerivative a owl:Class;
                                        rdfs:isDefinedBy fibo-der-drc-bsc:;
                                        rdfs:label "interest rate derivative";
                                        rdfs:subClassOf fibo-der-drc-bsc:RateBasedDerivative,
                                                        [a owl:Restriction ; 
                                                         owl:onProperty fibo-fbc-fi-fi:hasUnderlier ; 
                                                         owl:someValuesFrom [a owl:Restriction ; 
                                                                             owl:onProperty cmns-rlcmp:isPlayedBy ; 
                                                                             owl:someValuesFrom fibo-ind-ir-ir:ReferenceInterestRate]];
                                        skos:definition "rate-based derivative whose underlier is an interest rate";
                                        skos:example "For example, interest rate derivative strategies are the simultaneous trading of two or more rate contracts in which two counterparties agree to exchange interest rate cash flows on defined dates during an agreed period, based on a specified notional amount, from a fixed rate to a floating rate, floating to fixed, fixed to fixed, or floating to floating.";
                                        cmns-av:adaptedFrom "ISO 10962:2019, Securities and related financial instruments - Classification of financial instruments (CFI) code, Fourth edition, 2019-10".
fibo-der-drc-bsc:IntroducingBroker a owl:Class;
                                   rdfs:isDefinedBy fibo-der-drc-bsc:;
                                   rdfs:label "introducing broker";
                                   rdfs:subClassOf fibo-fbc-fct-fse:NonDepositoryInstitution,
                                                   fibo-fbc-pas-fpas:Broker;
                                   skos:definition "broker that solicits or accepts orders for derivatives that are traded on or subject to the rules of an exchange";
                                   cmns-av:abbreviation "IB";
                                   cmns-av:adaptedFrom "http://www.cftc.gov/IndustryOversight/Intermediaries/index.htm"^^xsd:anyURI;
                                   cmns-av:explanatoryNote "Introducing brokers do not accept money, securities, or property (or extend credit in lieu thereof) to margin, guarantee, or secure any trades or contracts that result or may result.".
fibo-der-drc-bsc:ObservableValue a owl:Class;
                                 rdfs:isDefinedBy fibo-der-drc-bsc:;
                                 rdfs:label "observable value";
                                 rdfs:seeAlso <https://en.wikipedia.org/wiki/Fair_value>;
                                 rdfs:subClassOf cmns-doc:Specification,
                                                 [a owl:Restriction ; 
                                                  owl:onProperty fibo-fbc-fi-fi:hasValueExpressedIn ; 
                                                  owl:onClass fibo-fnd-acc-cur:Currency ; 
                                                  owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                                 [a owl:Restriction ; 
                                                  owl:onProperty fibo-der-drc-bsc:specifiesValueOf ; 
                                                  owl:someValuesFrom owl:Thing];
                                 skos:definition "specification for the value for something discernible and for which evidence can be obtained";
                                 cmns-av:explanatoryNote "Derivatives, such as certain exotics, can be based on values ascribed to virtually anything, including weather. Typically, however, an observable value refers to something that can be readily observed in the marketplace, such as a quoted rate (e.g., interest rate, exchange rate), index value, commodity price, stock price, economic indicator, or something similar as of some point in time.".
fibo-der-drc-bsc:OverTheCounterDerivativeInstrument dcterms:source "ISO 4914:2021(en), Financial services - Unique product identifier (UPI)";
                                                    a owl:Class;
                                                    rdfs:isDefinedBy fibo-der-drc-bsc:;
                                                    rdfs:label "over-the-counter derivative instrument";
                                                    rdfs:subClassOf fibo-der-drc-bsc:OverTheCounterInstrument,
                                                                    fibo-fbc-fi-fi:DerivativeInstrument;
                                                    skos:definition "derivative instrument that is not listed on an organized exchange";
                                                    skos:note "ISO 4914 defines an OTC derivative instrument as a financial instrument that is, or would be, identified by an ISIN with the prefix 'EZ' or 'ZZ'. Details regarding how the prefix of an ISIN is determined can be found in ISO 6166:2020, Annex A.";
                                                    cmns-av:abbreviation "OTC derivative instrument".
fibo-der-drc-bsc:OverTheCounterInstrument a owl:Class;
                                          rdfs:isDefinedBy fibo-der-drc-bsc:;
                                          rdfs:label "over-the-counter instrument";
                                          rdfs:subClassOf fibo-fbc-fi-fi:FinancialInstrument;
                                          owl:disjointWith fibo-sec-sec-lst:ListedSecurity;
                                          skos:definition "financial instrument and bilateral contract that is not listed on an organized exchange";
                                          cmns-av:abbreviation "OTC instrument".
fibo-der-drc-bsc:ParametricCashflowTerms a owl:Class;
                                         rdfs:isDefinedBy fibo-der-drc-bsc:;
                                         rdfs:label "parametric cashflow terms";
                                         rdfs:subClassOf fibo-der-drc-bsc:CashflowTerms,
                                                         [a owl:Restriction ; 
                                                          owl:onProperty fibo-fnd-utl-alx:hasFormula ; 
                                                          owl:someValuesFrom fibo-der-drc-bsc:CashflowFormula];
                                         skos:definition "terms for a set of cashflows defined according to a mathematical formula".
fibo-der-drc-bsc:PayingParty a owl:Class;
                             rdfs:isDefinedBy fibo-der-drc-bsc:;
                             rdfs:label "paying party";
                             rdfs:subClassOf fibo-fnd-agr-ctr:ContractParty,
                                             fibo-fnd-pas-psch:Payer,
                                             fibo-fnd-pas-pas:Buyer;
                             skos:definition "party responsible for making payments in a transaction specified in a contract".
fibo-der-drc-bsc:RateBasedDerivative a owl:Class;
                                     rdfs:isDefinedBy fibo-der-drc-bsc:;
                                     rdfs:label "rate-based derivative";
                                     rdfs:subClassOf fibo-fbc-fi-fi:DerivativeInstrument,
                                                     [a owl:Restriction ; 
                                                      owl:onProperty fibo-fbc-fi-fi:hasUnderlier ; 
                                                      owl:someValuesFrom [a owl:Restriction ; 
                                                                          owl:onProperty cmns-rlcmp:isPlayedBy ; 
                                                                          owl:onClass [a owl:Class ; 
                                                                                       owl:unionOf _:autos738] ; 
                                                                          owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger]];
                                     skos:definition "derivative instrument where the holder has the right but may not have the obligation, depending on the nature of the instrument, to enter into the underlying contract, or pay or receive payment related to the underlying financial rate (or rate contract) on a specified future date based on a specified future rate and term";
                                     skos:example "Examples of rate-based derivatives include interest rate swaps, forward rate agreements (FRAs), and interest rate options such as caps, floors, and collars.";
                                     cmns-av:adaptedFrom "ISO 10962:2019, Securities and related financial instruments - Classification of financial instruments (CFI) code, Fourth edition, 2019-10";
                                     cmns-av:explanatoryNote "Rate-based derivatives derive their value from movements in some rate, e.g, an interest rate, market rate, economic indicator, statistical measure calculated over some collection of indices, rather than from traditional assets like stocks or commodities. They are commonly used by institutions to manage risks associated with interest rate fluctuations.".
fibo-der-drc-bsc:ReceivingParty a owl:Class;
                                rdfs:isDefinedBy fibo-der-drc-bsc:;
                                rdfs:label "receiving counterparty";
                                rdfs:subClassOf fibo-fnd-agr-ctr:ContractParty,
                                                fibo-fnd-pas-psch:Payee,
                                                fibo-fnd-pas-pas:Seller;
                                skos:definition "party that receives payments in a transaction specified in a contract".
fibo-der-drc-bsc:UnderlyingAssetValuation a owl:Class;
                                          rdfs:isDefinedBy fibo-der-drc-bsc:;
                                          rdfs:label "underlying asset valuation"@en;
                                          rdfs:subClassOf fibo-fnd-arr-asmt:ValueAssessment,
                                                          [a owl:Restriction ; 
                                                           owl:onProperty fibo-fbc-fi-fi:hasCalculationAgent ; 
                                                           owl:onClass fibo-fbc-fi-fi:CalculationAgent ; 
                                                           owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                                          [a owl:Restriction ; 
                                                           owl:onProperty fibo-fnd-rel-rel:evaluates ; 
                                                           owl:someValuesFrom fibo-fbc-fi-fi:Underlier];
                                          skos:definition "assessment activity to estimate the value of an underlying asset of a derivative"@en.
fibo-der-drc-bsc:UniqueProductIdentifier dcterms:source "ISO 4914:2021(en), Financial services - Unique product identifier (UPI)";
                                         a owl:Class;
                                         rdfs:isDefinedBy fibo-der-drc-bsc:;
                                         rdfs:label "unique product identifier";
                                         rdfs:subClassOf fibo-fbc-fi-fi:FinancialInstrumentIdentifier,
                                                         cmns-cxtid:StructuredIdentifier,
                                                         [a owl:Restriction ; 
                                                          owl:onProperty cmns-id:identifies ; 
                                                          owl:onClass fibo-der-drc-bsc:OverTheCounterDerivativeInstrument ; 
                                                          owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                                         [a owl:Restriction ; 
                                                          owl:onProperty cmns-ra:isRegisteredBy ; 
                                                          owl:someValuesFrom fibo-der-drc-bsc:UniqueProductIdentifierServiceProvider];
                                         skos:definition "sequence of characters uniquely identifying an OTC derivative product that is reportable to a trade repository";
                                         skos:scopeNote """At a minimum, the UPI code is applicable to OTC derivative instruments falling under the following categories of the Classification of Financial Instruments (ISO 10962):
		- Swaps (S)
		- Forwards (J)
		- Non-listed and complex listed options (H)
		- Others (miscellaneous) (M)""";
                                         cmns-av:abbreviation "UPI";
                                         cmns-av:explanatoryNote """The UPI code consists of 12 alphanumeric characters decomposed as follows:
		- the two-character prefix 'QZ'
		- nine alphanumeric characters (upper case A-Z and 0-9 only, excluding the vowel characters (A, E, I, O, U) and the character Y) without separators or special characters
		- one alphanumeric check character (A-Z and 0-9 only, excluding the vowel characters (A, E, I, O, U) and the character Y), calculated using the method specified in Annex C of the specification document.""".
fibo-der-drc-bsc:UniqueProductIdentifierReferenceDataLibrary a owl:NamedIndividual,
                                                               cmns-ra:Registry;
                                                             rdfs:isDefinedBy fibo-der-drc-bsc:;
                                                             rdfs:label "unique product identifier reference data library";
                                                             skos:definition "set of data comprised of reference data elements with specific values that together describe the product";
                                                             cmns-org:isManagedBy fibo-der-drc-bsc:UniqueProductIdentifierServiceProvider.
fibo-der-drc-bsc:UniqueProductIdentifierRegistryEntry a owl:Class;
                                                      rdfs:isDefinedBy fibo-der-drc-bsc:;
                                                      rdfs:label "unique product identifier registry entry";
                                                      rdfs:subClassOf cmns-ra:RegistryEntry,
                                                                      [a owl:Restriction ; 
                                                                       owl:onProperty cmns-cxtdsg:appliesTo ; 
                                                                       owl:allValuesFrom fibo-der-drc-bsc:OverTheCounterDerivativeInstrument],
                                                                      [a owl:Restriction ; 
                                                                       owl:onProperty cmns-col:isIncludedIn ; 
                                                                       owl:hasValue fibo-der-drc-bsc:UniqueProductIdentifierReferenceDataLibrary],
                                                                      [a owl:Restriction ; 
                                                                       owl:onProperty cmns-col:comprises ; 
                                                                       owl:someValuesFrom fibo-der-drc-bsc:UniqueProductIdentifier];
                                                      skos:definition "entry in a unique product identifier registry";
                                                      cmns-av:explanatoryNote "The Reference Data Library (RDL) is a set of reference data elements, together with their values, which is properly organized and maintained by the UPI service provider. The library associates UPI codes with the values of the reference data elements characterizing the product. Each entry in the library (the registry entry) contains a minimum number of elements as defined in the ISO standard, and may be extended by the service provider.".
fibo-der-drc-bsc:UniqueProductIdentifierServiceProvider dcterms:source "ISO 4914:2021(en), Financial services - Unique product identifier (UPI)";
                                                        a owl:Class;
                                                        rdfs:isDefinedBy fibo-der-drc-bsc:;
                                                        rdfs:label "unique product identifier service provider";
                                                        rdfs:subClassOf cmns-ra:RegistrationAuthority,
                                                                        [a owl:Restriction ; 
                                                                         owl:onProperty cmns-ra:registers ; 
                                                                         owl:onClass fibo-der-drc-bsc:UniqueProductIdentifier ; 
                                                                         owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger];
                                                        skos:definition "organization designated by an external body of financial regulators to assign UPIs and operate a UPI reference data library";
                                                        skos:scopeNote "At the time of publication of the ISO 4914 standard, there was only one such provider, the Regulatory Oversight Committee, confirmed by the Financial Stability Board as the International Governance Body for globally harmonised identifiers used to track OTC derivatives transactions.";
                                                        cmns-av:abbreviation "UPI service provider".
fibo-der-drc-bsc:ValuationTerms a owl:Class;
                                rdfs:isDefinedBy fibo-der-drc-bsc:;
                                rdfs:label "valuation terms";
                                rdfs:subClassOf fibo-der-drc-bsc:DerivativeTerms,
                                                [a owl:Restriction ; 
                                                 owl:onProperty fibo-fnd-arr-asmt:hasDateOfAssessment ; 
                                                 owl:someValuesFrom cmns-dt:ExplicitDate],
                                                [a owl:Restriction ; 
                                                 owl:onProperty fibo-fnd-rel-rel:involves ; 
                                                 owl:someValuesFrom fibo-der-drc-bsc:UnderlyingAssetValuation];
                                skos:definition "contract terms specific to valuation of the underlying asset(s)".
fibo-der-drc-bsc:hasAccretingNotionalAmount a owl:ObjectProperty;
                                            rdfs:isDefinedBy fibo-der-drc-bsc:;
                                            rdfs:label "has accreting notional amount"@en;
                                            rdfs:range fibo-fnd-acc-cur:MonetaryAmount;
                                            rdfs:subPropertyOf fibo-fnd-acc-cur:hasNotionalAmount;
                                            skos:definition "indicates that the notional amount increases through the life of the contract";
                                            cmns-av:adaptedFrom "ISO 10962:2019, Securities and related financial instruments - Classification of financial instruments (CFI) code, Fourth Edition, 2019-10, clause 6.8.2".
fibo-der-drc-bsc:hasAdditionalCosts a owl:ObjectProperty;
                                    rdfs:isDefinedBy fibo-der-drc-bsc:;
                                    rdfs:label "has additional costs"@en;
                                    rdfs:range fibo-fnd-acc-cur:MonetaryAmount;
                                    rdfs:subPropertyOf fibo-fnd-acc-cur:hasMonetaryAmount;
                                    skos:definition "indicates costs, such as up front costs, brokerage fees and the like, that must be paid on delivery"@en.
fibo-der-drc-bsc:hasAmortizingNotionalAmount a owl:ObjectProperty;
                                             rdfs:isDefinedBy fibo-der-drc-bsc:;
                                             rdfs:label "has amortizing notional amount"@en;
                                             rdfs:range fibo-fnd-acc-cur:MonetaryAmount;
                                             rdfs:subPropertyOf fibo-fnd-acc-cur:hasNotionalAmount;
                                             skos:definition "indicates that the notional amount decreases through the life of the contract";
                                             cmns-av:adaptedFrom "ISO 10962:2019, Securities and related financial instruments - Classification of financial instruments (CFI) code, Fourth Edition, 2019-10, clause 6.8.2".
fibo-der-drc-bsc:hasCalculationAgent a owl:ObjectProperty;
                                     rdfs:isDefinedBy fibo-der-drc-bsc:;
                                     owl:deprecated true;
                                     owl:equivalentProperty fibo-fbc-fi-fi:hasCalculationAgent.
fibo-der-drc-bsc:hasConstantNotionalAmount a owl:ObjectProperty;
                                           rdfs:isDefinedBy fibo-der-drc-bsc:;
                                           rdfs:label "has constant notional amount"@en;
                                           rdfs:range fibo-fnd-acc-cur:MonetaryAmount;
                                           rdfs:subPropertyOf fibo-fnd-acc-cur:hasNotionalAmount;
                                           skos:definition "indicates that the notional amount is constant through the life of the contract";
                                           cmns-av:adaptedFrom "ISO 10962:2019, Securities and related financial instruments - Classification of financial instruments (CFI) code, Fourth Edition, 2019-10, clause 6.8.2".
fibo-der-drc-bsc:hasCustomNotionalAmount a owl:ObjectProperty;
                                         rdfs:isDefinedBy fibo-der-drc-bsc:;
                                         rdfs:label "has custom notional amount"@en;
                                         rdfs:range fibo-fnd-acc-cur:MonetaryAmount;
                                         rdfs:subPropertyOf fibo-fnd-acc-cur:hasNotionalAmount;
                                         skos:definition "indicates that the notional amount is customized per a notional step schedule";
                                         cmns-av:adaptedFrom "ISO 10962:2019, Securities and related financial instruments - Classification of financial instruments (CFI) code, Fourth Edition, 2019-10, clause 6.8.2".
fibo-der-drc-bsc:hasFirstDeliveryDate a owl:ObjectProperty;
                                      rdfs:domain fibo-fbc-pas-fpas:SettlementTerms;
                                      rdfs:isDefinedBy fibo-der-drc-bsc:;
                                      rdfs:label "has first delivery date"@en;
                                      rdfs:range cmns-dt:ExplicitDate;
                                      rdfs:subPropertyOf cmns-dt:hasExplicitDate,
                                                         cmns-dt:hasStartDate;
                                      skos:definition "specifies the initial date in a range of dates by which the underlying asset (or some portion thereof) must be delivered in order for the terms of the contract to be fulfilled"@en.
fibo-der-drc-bsc:hasFirstNoticeDate a owl:ObjectProperty;
                                    rdfs:domain fibo-fbc-pas-fpas:SettlementTerms;
                                    rdfs:isDefinedBy fibo-der-drc-bsc:;
                                    rdfs:label "has first notice date"@en;
                                    rdfs:range cmns-dt:ExplicitDate;
                                    rdfs:subPropertyOf cmns-dt:hasExplicitDate,
                                                       cmns-dt:hasStartDate;
                                    skos:definition "specifies the initial date on which a delivery notice can be issued"@en.
fibo-der-drc-bsc:hasLastDeliveryDate a owl:ObjectProperty;
                                     rdfs:domain fibo-fbc-pas-fpas:SettlementTerms;
                                     rdfs:isDefinedBy fibo-der-drc-bsc:;
                                     rdfs:label "has last delivery date"@en;
                                     rdfs:range cmns-dt:ExplicitDate;
                                     rdfs:subPropertyOf cmns-dt:hasEndDate,
                                                        cmns-dt:hasExplicitDate;
                                     skos:definition "specifies the final date in a range of dates by which the underlying asset (or some portion thereof) must be delivered in order for the terms of the contract to be fulfilled"@en.
fibo-der-drc-bsc:hasLastNoticeDate a owl:ObjectProperty;
                                   rdfs:domain fibo-fbc-pas-fpas:SettlementTerms;
                                   rdfs:isDefinedBy fibo-der-drc-bsc:;
                                   rdfs:label "has last notice date"@en;
                                   rdfs:range cmns-dt:ExplicitDate;
                                   rdfs:subPropertyOf cmns-dt:hasEndDate,
                                                      cmns-dt:hasExplicitDate;
                                   skos:definition "specifies the final date on which a delivery notice can be issued"@en.
fibo-der-drc-bsc:hasSettlementTerms a owl:ObjectProperty;
                                    rdfs:domain fibo-fbc-fi-fi:DerivativeInstrument;
                                    rdfs:isDefinedBy fibo-der-drc-bsc:;
                                    rdfs:label "has settlement terms"@en;
                                    rdfs:range fibo-fbc-pas-fpas:SettlementTerms;
                                    rdfs:subPropertyOf fibo-fnd-agr-ctr:hasContractualElement;
                                    skos:definition "relates a derivative to contractual terms specific to the settlement process".
fibo-der-drc-bsc:hasTickValue a owl:ObjectProperty;
                              rdfs:domain fibo-fbc-fi-fi:DerivativeInstrument;
                              rdfs:isDefinedBy fibo-der-drc-bsc:;
                              rdfs:label "has tick value"@en;
                              rdfs:range fibo-fnd-acc-cur:MonetaryAmount;
                              rdfs:subPropertyOf fibo-fnd-acc-cur:hasMonetaryAmount;
                              skos:definition "indicates the cash value of one tick, i.e., the minimum price change of the contract"@en.
fibo-der-drc-bsc:hasUnderlyingAssetPrice a owl:ObjectProperty;
                                         rdfs:isDefinedBy fibo-der-drc-bsc:;
                                         rdfs:label "has underlying asset price";
                                         rdfs:range fibo-fnd-acc-cur:MonetaryPrice;
                                         rdfs:subPropertyOf fibo-fnd-acc-cur:hasPrice;
                                         skos:definition "specifies a price for something on which the contract is based".
fibo-der-drc-bsc:hasValuationTerms a owl:ObjectProperty;
                                   rdfs:domain fibo-fbc-fi-fi:DerivativeInstrument;
                                   rdfs:isDefinedBy fibo-der-drc-bsc:;
                                   rdfs:label "has valuation terms"@en;
                                   rdfs:range fibo-der-drc-bsc:ValuationTerms;
                                   rdfs:subPropertyOf fibo-fnd-agr-ctr:hasContractualElement;
                                   skos:definition "relates a derivative to contractual terms specific to valuation of the underlying asset(s)".
fibo-der-drc-bsc:specifiesValueOf a owl:ObjectProperty;
                                  rdfs:isDefinedBy fibo-der-drc-bsc:;
                                  rdfs:label "specifies value of"@en;
                                  rdfs:subPropertyOf cmns-cxtdsg:appliesTo,
                                                     cmns-doc:specifies;
                                  skos:definition "relates a derivative to contractual terms specific to valuation of the underlying asset(s)".
fibo-fbc-fi-fi:DerivativeInstrument a owl:Class;
                                    rdfs:subClassOf [a owl:Restriction ; 
                                                     owl:onProperty cmns-qtu:hasQuantityValue ; 
                                                     owl:onClass cmns-qtu:ScalarQuantityValue ; 
                                                     owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                                    [a owl:Restriction ; 
                                                     owl:onProperty fibo-der-drc-bsc:hasSettlementTerms ; 
                                                     owl:someValuesFrom fibo-fbc-pas-fpas:SettlementTerms],
                                                    [a owl:Restriction ; 
                                                     owl:onProperty fibo-der-drc-bsc:hasValuationTerms ; 
                                                     owl:someValuesFrom fibo-der-drc-bsc:ValuationTerms],
                                                    [a owl:Restriction ; 
                                                     owl:onProperty fibo-fbc-fi-fi:hasUnderlier ; 
                                                     owl:someValuesFrom fibo-fbc-fi-fi:Underlier],
                                                    [a owl:Restriction ; 
                                                     owl:onProperty fibo-fnd-arr-doc:hasExpirationDate ; 
                                                     owl:someValuesFrom cmns-dt:Date];
                                    cmns-av:usageNote "Note that the quantity value associated with the derivative represents the quantity of the underlier. The price of the underlier is included in settlement terms.".
fibo-fbc-fi-fi:Underlier a owl:Class;
                         rdfs:subClassOf [a owl:Restriction ; 
                                          owl:onProperty cmns-rlcmp:isPlayedBy ; 
                                          owl:someValuesFrom [a owl:Class ; 
                                                              owl:unionOf _:autos759]].
