_:autos1215 rdf:first fibo-fbc-dae-gty:Guaranty;
            rdf:rest rdf:nil.
fibo-fbc-dae-dbt:CreditEnhancementAgreement a owl:Class;
                                            rdfs:subClassOf [a owl:Restriction ; 
                                                             owl:onProperty fibo-fbc-dae-dbt:isCollateralizedBy ; 
                                                             owl:someValuesFrom [a owl:Class ; 
                                                                                 owl:unionOf _:autos1214]].
fibo-fbc-dae-gty: dcterms:abstract "This ontology defines concepts related to contractual guaranty.";
