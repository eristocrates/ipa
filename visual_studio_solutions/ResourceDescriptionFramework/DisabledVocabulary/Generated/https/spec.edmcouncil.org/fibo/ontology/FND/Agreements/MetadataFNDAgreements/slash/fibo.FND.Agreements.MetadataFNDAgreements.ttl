                  dcterms:license "https://opensource.org/licenses/MIT"^^xsd:anyURI;
                  dcterms:modified "2023-01-20T18:00:00"^^xsd:dateTime;
                  a owl:Ontology;
                  rdfs:label "Metadata for the EDMC-FIBO Foundations (FND) Agreements Module";
