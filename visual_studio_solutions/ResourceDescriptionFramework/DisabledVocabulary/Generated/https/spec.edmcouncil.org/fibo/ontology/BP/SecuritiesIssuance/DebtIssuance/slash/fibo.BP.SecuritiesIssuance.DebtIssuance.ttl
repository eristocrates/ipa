_:autos387 rdf:first fibo-der-drc-raw:TraditionalWarrant;
           rdf:rest _:autos388.
_:autos388 rdf:first fibo-sec-dbt-bnd:Bond;
           rdf:rest rdf:nil.
fibo-bp-iss-dbti: dcterms:abstract "General issuance process for issuance of debt instruments. Forms the basis for more detailed issuance processes such as MBS issuance and municipal bonds issue.";
                  dcterms:license "https://opensource.org/licenses/MIT"^^xsd:anyURI;
                  a owl:Ontology;
                  rdfs:label "Debt Issuance Ontology"@en;
                  owl:imports <https://spec.edmcouncil.org/fibo/ontology/master/latest/BP/SecuritiesIssuance/IssuanceProcess/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/BP/SecuritiesIssuance/MuniIssuance/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/BP/SecuritiesIssuance/PrivateLabelMBSIssuance/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/DER/DerivativesContracts/RightsAndWarrants/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/ProductsAndServices/FinancialProductsAndServices/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Accounting/CurrencyAmount/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Agreements/Contracts/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/Lifecycles/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/GoalsAndObjectives/Objectives/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Relations/Relations/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/AnnotationVocabulary/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/SEC/Debt/Bonds/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/SEC/Debt/DebtInstruments/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/SEC/Debt/PoolBackedSecurities/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/SEC/Securities/Pools/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/SEC/Securities/SecuritiesIssuance/>,
                              cmns-av:,
                              cmns-col:,
                              cmns-cxtdsg:,
                              cmns-dt:,
                              cmns-doc:,
                              cmns-qtu:;
                  owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/latest/BP/SecuritiesIssuance/DebtIssuance/>;
                  fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Provisional;
                  cmns-av:copyright "Copyright (c) 2013-2023 EDM Council, Inc.".
fibo-bp-iss-dbti:ABSSecuritizationProcess a owl:Class;
                                          rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                          rdfs:label "a b s securitization process"@en;
                                          rdfs:subClassOf fibo-bp-iss-dbti:PoolBackedSecuritySecuritizationProcess.
fibo-bp-iss-dbti:AdvancedRefunding a owl:Class;
                                   rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                   rdfs:label "advance refunding"@en;
                                   rdfs:subClassOf fibo-bp-iss-dbti:RefundingPurpose;
                                   skos:definition "refunding in which bond issuance in which new bonds are sold at a lower rate than outstanding ones"@en;
                                   cmns-av:explanatoryNote "The proceeds are then invested, and when the older bonds become callable they are paid off with the invested proceeds."@en.
fibo-bp-iss-dbti:AssetPoolCreationProcess a owl:Class;
                                          rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                          rdfs:label "asset pool creation process"@en;
                                          rdfs:subClassOf [a owl:Restriction ; 
                                                           owl:onProperty cmns-dt:precedes ; 
                                                           owl:someValuesFrom fibo-bp-iss-dbti:DebtSecuritizationProcess].
fibo-bp-iss-dbti:BondIssuanceProgramme a owl:Class;
                                       rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                       rdfs:label "bond issuance programme"@en;
                                       rdfs:subClassOf fibo-bp-iss-dbti:DebtIssuanceProgramme,
                                                       [a owl:Restriction ; 
                                                        owl:onProperty cmns-col:comprises ; 
                                                        owl:someValuesFrom fibo-bp-iss-dbti:BondOffering];
                                       skos:definition "a debt issuance programe under which an entity may, from time to time, issue bonds under the terms and conditions specified in the base prospectus for that programme"@en.
fibo-bp-iss-dbti:BondOffering a owl:Class;
                              rdfs:isDefinedBy fibo-bp-iss-dbti:;
                              rdfs:label "bond offering"@en;
                              rdfs:subClassOf fibo-sec-dbt-dbti:DebtOffering;
                              skos:definition "a debt offering of one or more bonds, as all or part of an issuance programme"@en.
fibo-bp-iss-dbti:BondOfferingWithWarrant a owl:Class;
                                         rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                         rdfs:label "bond offering with warrant"@en;
                                         rdfs:subClassOf fibo-bp-iss-dbti:BondOffering,
                                                         [a owl:Restriction ; 
                                                          owl:onProperty cmns-cxtdsg:appliesTo ; 
                                                          owl:someValuesFrom [a owl:Class ; 
                                                                              owl:unionOf _:autos387]];
                                         owl:disjointWith fibo-bp-iss-dbti:MediumTermNoteOffering;
                                         skos:definition """a bond offering where the issue includes a warrant; Further notes: ISO 10962 CFI definition is A bond that is issued together with one or more warrant(s) attached as part of the offer, the warrant(s) granting the holder the right to purchase a designated security, often the common stock of the issuer of the debt, at a specified price. Review notes: This need not be any specific type of bond. The warrant is used as a sweetener to encourage people to subscribe to a new bond issue. The Bond and the Warrant trade together as a unit (called \"Bond Unit\")."""@en.
fibo-bp-iss-dbti:CDOSecuritizationProcess a owl:Class;
                                          rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                          rdfs:label "c d o securitization process"@en;
                                          rdfs:subClassOf fibo-bp-iss-dbti:DebtSecuritizationProcess.
fibo-bp-iss-dbti:CompletedDebtsPool a owl:Class;
                                    rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                    rdfs:label "completed debts pool"@en;
                                    rdfs:subClassOf fibo-sec-sec-pls:DebtPool,
                                                    [a owl:Restriction ; 
                                                     owl:onProperty fibo-fnd-arr-lif:hasStage ; 
                                                     owl:someValuesFrom fibo-bp-iss-dbti:InIssuance].
fibo-bp-iss-dbti:CreditCardPoolCreationProcess a owl:Class;
                                               rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                               rdfs:label "credit card pool creation process"@en;
                                               rdfs:subClassOf fibo-bp-iss-dbti:RetailAssetPoolCreationProcess.
fibo-bp-iss-dbti:DebtInstrumentPoolCreationProcess a owl:Class;
                                                   rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                                   rdfs:label "debt instrument pool creation process"@en;
                                                   rdfs:subClassOf fibo-bp-iss-dbti:AssetPoolCreationProcess.
fibo-bp-iss-dbti:DebtIssuanceProcessInformation a owl:Class;
                                                rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                                rdfs:label "debt issuance process information"@en;
                                                rdfs:subClassOf fibo-bp-iss-prc:TradedInstrumentIssuanceProcessInformation,
                                                                [a owl:Restriction ; 
                                                                 owl:onProperty fibo-bp-iss-dbti:hasDebtIssuancePurpose ; 
                                                                 owl:someValuesFrom fibo-bp-iss-dbti:DebtIssuancePurpose];
                                                skos:definition "information specific to the issuance of a debt security"@en.
fibo-bp-iss-dbti:DebtIssuanceProgramme a owl:Class;
                                       rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                       rdfs:label "debt issuance programme"@en;
                                       rdfs:subClassOf fibo-bp-iss-prc:IssuanceProgramme,
                                                       [a owl:Restriction ; 
                                                        owl:onProperty cmns-col:comprises ; 
                                                        owl:someValuesFrom fibo-sec-dbt-dbti:DebtOffering];
                                       skos:definition "a series of debt security issuances over time"@en.
fibo-bp-iss-dbti:DebtIssuancePurpose a owl:Class;
                                     rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                     rdfs:label "debt issuance purpose"@en;
                                     rdfs:subClassOf fibo-fnd-gao-obj:Objective;
                                     skos:definition "purpose for the issuance of a debt security; also defines processes to be followed in some instances"@en.
fibo-bp-iss-dbti:DebtSecuritizationProcess a owl:Class;
                                           rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                           rdfs:label "debt securitization process"@en;
                                           rdfs:subClassOf fibo-bp-iss-prc:SecuritiesIssuanceProcess;
                                           owl:disjointWith fibo-bp-iss-muni:DebtAuctionProcess,
                                                            fibo-bp-iss-muni:SecuritiesUnderwritingIssuanceProcess.
fibo-bp-iss-dbti:HomeEquityLineOfCreditPoolCreationProcess a owl:Class;
                                                           rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                                           rdfs:label "home equity line of credit pool creation process"@en;
                                                           rdfs:subClassOf fibo-bp-iss-dbti:RetailAssetPoolCreationProcess.
fibo-bp-iss-dbti:InAssembly a owl:Class;
                            rdfs:isDefinedBy fibo-bp-iss-dbti:;
                            rdfs:label "in assembly"@en;
                            rdfs:subClassOf fibo-bp-iss-pmbs:PoolLifecycleStateSelection.
fibo-bp-iss-dbti:InAssemblyDebtsPool a owl:Class;
                                     rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                     rdfs:label "in assembly debts pool"@en;
                                     rdfs:subClassOf fibo-sec-sec-pls:DebtPool,
                                                     [a owl:Restriction ; 
                                                      owl:onProperty fibo-fnd-arr-lif:hasStage ; 
                                                      owl:someValuesFrom fibo-bp-iss-dbti:InAssembly].
fibo-bp-iss-dbti:InIssuance a owl:Class;
                            rdfs:isDefinedBy fibo-bp-iss-dbti:;
                            rdfs:label "in issuance"@en;
                            rdfs:subClassOf fibo-bp-iss-pmbs:PoolLifecycleStateSelection.
fibo-bp-iss-dbti:IssuanceProcessActivity a owl:Class;
                                         rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                         rdfs:label "issuance process activity"@en;
                                         skos:definition "An activity within the process of securities issuance."@en.
fibo-bp-iss-dbti:Issued a owl:Class;
                        rdfs:isDefinedBy fibo-bp-iss-dbti:;
                        rdfs:label "issued"@en;
                        rdfs:subClassOf fibo-bp-iss-pmbs:PoolLifecycleStateSelection.
fibo-bp-iss-dbti:LoanPoolCreationProcess a owl:Class;
                                         rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                         rdfs:label "loan pool creation process"@en;
                                         rdfs:subClassOf fibo-bp-iss-dbti:RetailAssetPoolCreationProcess.
fibo-bp-iss-dbti:MTNRegistration a owl:Class;
                                 rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                 rdfs:label "m t n registration"@en;
                                 rdfs:subClassOf fibo-bp-iss-muni:Registration,
                                                 [a owl:Restriction ; 
                                                  owl:onProperty cmns-doc:isAbout ; 
                                                  owl:someValuesFrom fibo-bp-iss-dbti:MediumTermNoteIssuanceProgramme].
fibo-bp-iss-dbti:MediumTermNoteIssuanceProgramme a owl:Class;
                                                 rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                                 rdfs:label "medium term note issuance programme"@en;
                                                 rdfs:subClassOf fibo-bp-iss-dbti:BondIssuanceProgramme,
                                                                 [a owl:Restriction ; 
                                                                  owl:onProperty cmns-col:comprises ; 
                                                                  owl:someValuesFrom fibo-bp-iss-dbti:MediumTermNoteOffering];
                                                 skos:definition "a program of offerings of medium term notes; a set of issues where the maturity is defined after the rest of the terms have been registered with some authority; these are registered up front so that then the company wants to borrow more money they don't have to go through the registration period but have the facility up front to issue another security."@en.
fibo-bp-iss-dbti:MediumTermNoteOffering a owl:Class;
                                        rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                        rdfs:label "medium term note offering"@en;
                                        rdfs:subClassOf fibo-bp-iss-dbti:BondOffering.
fibo-bp-iss-dbti:MuniIssueUnderwriter a owl:Class;
                                      rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                      rdfs:label "muni issue underwriter"@en;
                                      rdfs:subClassOf fibo-sec-sec-iss:SecurityUnderwriter;
                                      skos:definition "The party which agrees to buy any certificates that are not bought by investors Term origin:DTCC"@en.
fibo-bp-iss-dbti:NewMoneyPurpose a owl:Class;
                                 rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                 rdfs:label "new money purpose"@en;
                                 rdfs:subClassOf fibo-bp-iss-dbti:DebtIssuancePurpose;
                                 skos:definition "securities issued to raise funds for a new project or purpose"@en.
fibo-bp-iss-dbti:NotYetIssued a owl:Class;
                              rdfs:isDefinedBy fibo-bp-iss-dbti:;
                              rdfs:label "not yet issued"@en;
                              rdfs:subClassOf fibo-bp-iss-pmbs:PoolLifecycleStateSelection.
fibo-bp-iss-dbti:NotYetIssuedDebtsPool a owl:Class;
                                       rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                       rdfs:label "not yet issued debts pool"@en;
                                       rdfs:subClassOf fibo-sec-sec-pls:DebtPool,
                                                       [a owl:Restriction ; 
                                                        owl:onProperty fibo-fnd-arr-lif:hasStage ; 
                                                        owl:someValuesFrom fibo-bp-iss-dbti:NotYetIssued].
fibo-bp-iss-dbti:PoolBackedSecuritySecuritizationProcess a owl:Class;
                                                         rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                                         rdfs:label "pool backed security securitization process"@en;
                                                         rdfs:subClassOf fibo-bp-iss-dbti:DebtSecuritizationProcess.
fibo-bp-iss-dbti:PoolBackedSecuritySecuritizationProcessActor a owl:Class;
                                                              rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                                              rdfs:label "pool backed security securitization process actor"@en;
                                                              rdfs:subClassOf fibo-bp-iss-dbti:SecuritizationProcessActor,
                                                                              [a owl:Restriction ; 
                                                                               owl:onProperty fibo-fnd-rel-rel:evaluates ; 
                                                                               owl:someValuesFrom fibo-bp-iss-prc:IssuanceProcessActivity].
fibo-bp-iss-dbti:RefundingPurpose a owl:Class;
                                  rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                  rdfs:label "refunding purpose"@en;
                                  rdfs:subClassOf fibo-bp-iss-dbti:DebtIssuancePurpose;
                                  skos:definition "a procedure whereby an issuer refinances outstanding bonds by issuing new bonds"@en.
fibo-bp-iss-dbti:RemarketingPurpose a owl:Class;
                                    rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                    rdfs:label "remarketing purpose"@en;
                                    rdfs:subClassOf fibo-bp-iss-dbti:DebtIssuancePurpose;
                                    skos:definition "the process of reselling securities to the public that have been tendered for purchase by the previous owners thereof"@en.
fibo-bp-iss-dbti:RetailAssetPoolCreationProcess a owl:Class;
                                                rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                                rdfs:label "retail asset pool creation process"@en;
                                                rdfs:subClassOf fibo-bp-iss-dbti:AssetPoolCreationProcess,
                                                                [a owl:Restriction ; 
                                                                 owl:onProperty fibo-fnd-rel-rel:isProducedBy ; 
                                                                 owl:someValuesFrom fibo-sec-sec-pls:DebtPool],
                                                                [a owl:Restriction ; 
                                                                 owl:onProperty cmns-dt:precedes ; 
                                                                 owl:someValuesFrom fibo-bp-iss-dbti:PoolBackedSecuritySecuritizationProcess];
                                                skos:definition "The process by which pools of assets are created. These may then be used in the issue of securities based on those asset pools as underlying."@en.
fibo-bp-iss-dbti:SecuritizationProcessActor a owl:Class;
                                            rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                            rdfs:label "securitization process actor"@en;
                                            rdfs:subClassOf fibo-bp-iss-prc:SecuritiesIssuanceProcessActor.
fibo-bp-iss-dbti:UnderwriterTakedownForDebt a owl:Class;
                                            rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                            rdfs:label "underwriter takedown for debt"@en;
                                            rdfs:subClassOf fibo-bp-iss-prc:UnderwriterTakedown;
                                            skos:definition "information on takedown quantity of the debt security handled by the underwriter (that will be brought into DTC)"@en.
fibo-bp-iss-dbti:announces a owl:ObjectProperty;
                           rdfs:isDefinedBy fibo-bp-iss-dbti:;
                           rdfs:label "announces"@en;
                           skos:definition "The issue of one or more securities that the announcement pertains to."@en.
fibo-bp-iss-dbti:hasDebtIssuancePurpose a owl:ObjectProperty;
                                        rdfs:domain fibo-bp-iss-dbti:DebtIssuanceProcessInformation;
                                        rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                        rdfs:label "has debt issuance purpose"@en;
                                        rdfs:range fibo-bp-iss-dbti:DebtIssuancePurpose.
fibo-bp-iss-dbti:hasOriginalIssueDiscount a owl:ObjectProperty;
                                          rdfs:domain fibo-bp-iss-dbti:BondOffering;
                                          rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                          rdfs:label "has original issue discount"@en;
                                          rdfs:range cmns-qtu:Percentage;
                                          skos:definition "discount from par value at the time a bond or other debt instrument is issued"@en;
                                          cmns-av:explanatoryNote "The OID is the difference between the stated redemption price at maturity and the actual issue price."@en.
fibo-bp-iss-dbti:isBankQualified a owl:DatatypeProperty;
                                 rdfs:domain fibo-bp-iss-dbti:BondOffering;
                                 rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                 rdfs:label "is bank qualified"@en;
                                 rdfs:range xsd:boolean;
                                 skos:definition "designation given to a public purpose bond offering by the issuer if it reasonably expects to issue in the calendar year of such offering no more than $10 million par amount of bonds of the type required to be included in making such calculation under the Internal Revenue Code.  When purchased by a commercial bank for its portfolio, the bank may receive an 80% tax deduction for the interest cost of carry for the issue.  A bond that is bank qualified is also known as a qualified tax-exempt obligation."@en.
fibo-bp-iss-dbti:resultsIn a owl:ObjectProperty;
                           rdfs:isDefinedBy fibo-bp-iss-dbti:;
                           rdfs:label "results in"@en;
                           rdfs:range fibo-bp-iss-pmbs:PotentialTranchedIssueUnderwriter.
fibo-bp-iss-dbti:underwriterTakedownAmount a owl:ObjectProperty;
                                           rdfs:domain fibo-bp-iss-dbti:UnderwriterTakedownForDebt;
                                           rdfs:isDefinedBy fibo-bp-iss-dbti:;
                                           rdfs:label "underwriter takedown amount"@en;
                                           rdfs:range fibo-fnd-acc-cur:MonetaryAmount;
                                           skos:definition "Takedown amount of the security handled by the underwriter(that will be brought into DTC)."@en.
fibo-sec-dbt-dbti:DebtOffering a owl:Class;
                               rdfs:subClassOf [a owl:Restriction ; 
                                                owl:onProperty fibo-fbc-pas-fpas:hasOfferingAmount ; 
                                                owl:onClass fibo-fnd-acc-cur:MonetaryAmount ; 
                                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger],
                                               [a owl:Restriction ; 
                                                owl:onProperty fibo-fnd-agr-ctr:isEvidencedBy ; 
                                                owl:someValuesFrom fibo-sec-sec-iss:Prospectus],
                                               [a owl:Restriction ; 
                                                owl:onProperty fibo-sec-sec-iss:isUnderwrittenBy ; 
                                                owl:someValuesFrom fibo-sec-sec-iss:SecurityUnderwriter],
                                               [a owl:Restriction ; 
                                                owl:onProperty cmns-cxtdsg:appliesTo ; 
                                                owl:someValuesFrom fibo-sec-dbt-dbti:TradableDebtInstrument].
