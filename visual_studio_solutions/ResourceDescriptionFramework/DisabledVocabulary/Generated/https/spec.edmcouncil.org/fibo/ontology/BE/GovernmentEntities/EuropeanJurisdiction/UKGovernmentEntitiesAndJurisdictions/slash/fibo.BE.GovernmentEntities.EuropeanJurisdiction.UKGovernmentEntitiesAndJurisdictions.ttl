                                           cmns-rga:isJurisdictionOf fibo-be-ge-ukj:NationalAssemblyForWales.
fibo-be-ge-ukj:GovernmentOfGuernsey a owl:NamedIndividual,
                                      fibo-be-ge-ge:RegionalGovernment;
