_:autos651 rdf:first fibo-ind-mkt-bas:BasketOfEquities;
           rdf:rest _:autos652.
_:autos652 rdf:first fibo-fbc-fi-fi:EquityInstrument;
           rdf:rest _:autos653.
_:autos653 rdf:first fibo-ind-mkt-bas:EquityIndex;
           rdf:rest rdf:nil.
_:autos660 rdf:first fibo-ind-mkt-bas:BasketOfEquities;
           rdf:rest _:autos661.
_:autos661 rdf:first fibo-fbc-fi-fi:EquityInstrument;
           rdf:rest _:autos662.
_:autos662 rdf:first fibo-ind-mkt-bas:EquityIndex;
           rdf:rest rdf:nil.
_:autos665 rdf:first fibo-der-sbd-eqs:EquityReturnLeg;
           rdf:rest _:autos666.
_:autos666 rdf:first fibo-der-drc-swp:TotalReturnLeg;
           rdf:rest rdf:nil.
