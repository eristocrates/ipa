                 dcterms:license "https://opensource.org/licenses/MIT"^^xsd:anyURI;
                 dcterms:modified "2023-01-20T18:00:00"^^xsd:dateTime;
                 a owl:Ontology;
                 rdfs:label "Metadata for the EDMC-FIBO Foundations (FND) Dates and Times Module";
                 owl:imports <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/AnnotationVocabulary/>,
                             cmns-av:;
                 owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/DatesAndTimes/MetadataFNDDatesAndTimes/>;
