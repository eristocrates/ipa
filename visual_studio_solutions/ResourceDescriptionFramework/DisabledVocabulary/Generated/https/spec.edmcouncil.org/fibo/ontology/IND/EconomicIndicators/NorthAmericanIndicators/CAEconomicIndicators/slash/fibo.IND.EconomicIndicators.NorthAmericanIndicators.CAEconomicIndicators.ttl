fibo-ind-ei-caei:CanadianConsumerPriceIndex a owl:Class;
                                            rdfs:isDefinedBy fibo-ind-ei-caei:;
                                            rdfs:label "Canadian consumer price index";
                                            rdfs:subClassOf fibo-ind-ei-ei:ConsumerPriceIndex,
                                                            [a owl:Restriction ; 
                                                             owl:onProperty fibo-be-fct-pub:hasPublisher ; 
                                                             owl:hasValue fibo-ind-ei-caei:CanadianStatisticsPublisher],
                                                            [a owl:Restriction ; 
                                                             owl:onProperty fibo-be-fct-pub:isPublishedBy ; 
                                                             owl:hasValue fibo-ind-ei-caei:StatisticsCanada],
                                                            [a owl:Restriction ; 
                                                             owl:onProperty cmns-qtu:hasArgument ; 
                                                             owl:onClass fibo-ind-ei-caei:CanadianHouseholdsConsumersUniverse ; 
                                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger],
                                                            [a owl:Restriction ; 
                                                             owl:onProperty cmns-cxtdsg:appliesTo ; 
                                                             owl:someValuesFrom fibo-ind-ei-caei:CanadianHouseholdsConsumersUniverse],
                                                            [a owl:Restriction ; 
                                                             owl:onProperty cmns-qtu:hasArgument ; 
                                                             owl:someValuesFrom fibo-ind-ei-ei:FixedBasket];
                                            skos:definition "economic indicator representing a measure of changes over time in the prices of a fixed basket of consumer goods and services that Canadian private households consume";
                                            cmns-av:abbreviation "CPI";
                                            cmns-av:adaptedFrom "http://www.statcan.gc.ca/eng/start"^^xsd:anyURI,
                                                                "https://www.statcan.gc.ca/en/subjects-start/prices_and_price_indexes/consumer_price_indexes"^^xsd:anyURI.
fibo-ind-ei-caei:CanadianHouseholdsConsumersUniverse a owl:Class;
                                                     rdfs:isDefinedBy fibo-ind-ei-caei:;
                                                     rdfs:label "Canadian households consumers universe";
                                                     rdfs:subClassOf fibo-ind-ei-ei:CivilianNonInstitutionalPopulation,
                                                                     [a owl:Restriction ; 
                                                                      owl:onProperty cmns-col:hasMember ; 
                                                                      owl:someValuesFrom [a owl:Restriction ; 
                                                                                          owl:onProperty cmns-rlcmp:playsRole ; 
                                                                                          owl:someValuesFrom fibo-ind-ei-ei:UltimateConsumer]],
                                                                     [a owl:Restriction ; 
                                                                      owl:onProperty cmns-cxtdsg:isApplicableIn ; 
                                                                      owl:someValuesFrom fibo-ind-ei-ei:Household];
                                                     skos:definition "a statistical universe consisting of all private households in Canada, with the exception of soldiers on military bases, people living on First Nations reserves, institutionalized persons, and households living in the rural areas of the three northern territories";
                                                     cmns-av:adaptedFrom "http://www.statcan.gc.ca/pub/62-553-x/62-553-x2015001-eng.pdf".
fibo-ind-ei-caei:CanadianProducerPriceIndex a owl:Class;
                                            rdfs:isDefinedBy fibo-ind-ei-caei:;
                                            rdfs:label "Canadian producer price index";
                                            rdfs:subClassOf fibo-ind-ei-ei:ProducerPriceIndex,
                                                            [a owl:Restriction ; 
                                                             owl:onProperty fibo-be-fct-pub:hasPublisher ; 
                                                             owl:hasValue fibo-ind-ei-caei:CanadianStatisticsPublisher],
                                                            [a owl:Restriction ; 
                                                             owl:onProperty fibo-be-fct-pub:isPublishedBy ; 
                                                             owl:hasValue fibo-ind-ei-caei:StatisticsCanada],
                                                            [a owl:Restriction ; 
                                                             owl:onProperty cmns-cxtdsg:appliesTo ; 
                                                             owl:someValuesFrom [a owl:Class ; 
                                                                                 owl:unionOf _:autos2648]];
                                            skos:definition "an economic indicator representing a measure of the change over time in the prices of a fixed-basket of domestic producer goods and services";
                                            cmns-av:adaptedFrom "http://www23.statcan.gc.ca/imdb-bmdi/pub/indexth-eng.htm"^^xsd:anyURI;
                                            cmns-av:explanatoryNote "Note that Canada does not produce a high level, cross industry PPI per se. Canadian PPIs are published by industry sector. Three of the most important are captured in the union defined herein, which may be expanded over time to integrate others, as needed.".
fibo-ind-ei-caei:CanadianStatisticsPublisher a owl:NamedIndividual,
                                               fibo-ind-ei-ei:StatisticalInformationPublisher;
                                             rdfs:isDefinedBy fibo-ind-ei-caei:;
                                             rdfs:label "Canadian statistics publisher";
                                             skos:definition "individual representing the functional entity that is the primary publisher of statistical information for the Canadian Government";
                                             cmns-av:adaptedFrom "http://www.statcan.gc.ca/eng/about/mandate/"^^xsd:anyURI;
                                             cmns-rlcmp:isPlayedBy fibo-ind-ei-caei:StatisticsCanada.
fibo-ind-ei-caei:IndustrialProductsSector a owl:Class;
                                          rdfs:isDefinedBy fibo-ind-ei-caei:;
                                          rdfs:label "industrial products sector";
                                          rdfs:subClassOf fibo-fnd-arr-cls:IndustrySectorClassifier;
                                          skos:definition "a classifier for a sector of the economy used for price indices focused on major commodities sold by manufacturers in Canada";
                                          cmns-av:adaptedFrom "http://www23.statcan.gc.ca/imdb/p2SV.pl?Function=getSurvey&SDDS=2318"^^xsd:anyURI.
fibo-ind-ei-caei:NewHousingSector a owl:Class;
                                  rdfs:isDefinedBy fibo-ind-ei-caei:;
                                  rdfs:label "new housing sector";
                                  rdfs:subClassOf fibo-fnd-arr-cls:IndustrySectorClassifier;
                                  skos:definition "a classifier for a sector of the economy used for price indices focused on contractors' selling prices of new residential houses, where detailed specifications pertaining to each house remain the same between two consecutive periods";
                                  cmns-av:adaptedFrom "http://www23.statcan.gc.ca/imdb/p2SV.pl?Function=getSurvey&SDDS=2310"^^xsd:anyURI.
fibo-ind-ei-caei:RawMaterialsSector a owl:Class;
                                    rdfs:isDefinedBy fibo-ind-ei-caei:;
                                    rdfs:label "raw materials sector";
                                    rdfs:subClassOf fibo-fnd-arr-cls:IndustrySectorClassifier;
                                    skos:definition "a classifier used for price indices related to raw materials purchased by industries in Canada for further processing";
                                    cmns-av:adaptedFrom "http://www23.statcan.gc.ca/imdb/p2SV.pl?Function=getSurvey&SDDS=2306"^^xsd:anyURI.
fibo-ind-ei-caei:StatisticsCanada a owl:NamedIndividual,
                                    fibo-be-ge-ge:GovernmentAgency;
                                  rdfs:isDefinedBy fibo-ind-ei-caei:;
                                  rdfs:label "Statistics Canada"@en,
                                             "Statistique Canada"@fr;
                                  skos:definition "individual representing Statistics Canada, a government agency mandated to collect, compile, analyse, abstract and publish statistical information relating to the commercial, industrial, financial, social, economic and general activities and condition of the people of Canada";
                                  cmns-av:abbreviation "StatCan";
                                  cmns-av:adaptedFrom "http://www.statcan.gc.ca/eng/about/mandate"^^xsd:anyURI;
                                  cmns-col:isPartOf fibo-be-ge-caj:GovernmentOfCanada;
                                  cmns-loc:hasCoverageArea lcc-3166-1:Canada.
