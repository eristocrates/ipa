                 a owl:Ontology;
                 rdfs:label "Loan Events Ontology"@en;
                 owl:imports <https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/DebtAndEquities/Debt/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Accounting/CurrencyAmount/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/Assessments/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/Lifecycles/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/DatesAndTimes/Occurrences/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Law/LegalCore/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Parties/Parties/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/ProductsAndServices/PaymentsAndSchedules/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Relations/Relations/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/AnnotationVocabulary/>,
                             <https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoansGeneral/Loans/>,
                             cmns-av:,
                             cmns-dt:,
                             cmns-org:,
                             cmns-rga:;
                 owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoansGeneral/LoanEvents/>;
                 fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Provisional;
                 cmns-av:copyright "Copyright (c) 2016-2025 EDM Council, Inc.",
                                   "Copyright (c) 2016-2025 Object Management Group, Inc.".
fibo-loan-ln-ev:CollateralValuation a owl:Class;
                                    rdfs:isDefinedBy fibo-loan-ln-ev:;
                                    rdfs:label "collateral valuation"@en;
                                    rdfs:subClassOf fibo-fnd-arr-asmt:AssessmentActivity,
                                                    [a owl:Restriction ; 
                                                     owl:onProperty fibo-fnd-rel-rel:evaluates ; 
                                                     owl:allValuesFrom fibo-fbc-dae-dbt:Collateral],
                                                    [a owl:Restriction ; 
                                                     owl:onProperty cmns-org:isProvidedBy ; 
                                                     owl:someValuesFrom fibo-fnd-arr-asmt:Appraiser];
                                    skos:definition "assessment activity resulting in the valuation of real property as collateral"@en.
fibo-loan-ln-ev:CourtJudgment a owl:Class;
                              rdfs:isDefinedBy fibo-loan-ln-ev:;
                              rdfs:label "court judgment"@en;
                              rdfs:subClassOf fibo-loan-ln-ev:LegalProceeding,
                                              [a owl:Restriction ; 
                                               owl:onProperty fibo-loan-ln-ev:hasJudgementAmount ; 
                                               owl:onClass fibo-fnd-acc-cur:MonetaryAmount ; 
                                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                              [a owl:Restriction ; 
                                               owl:onProperty fibo-loan-ln-ev:isDeliveredBy ; 
                                               owl:someValuesFrom fibo-fnd-law-cor:CourtOfLaw];
                              skos:definition "decision by a court or other tribunal that resolves a controversy and determines the rights and obligations of the parties"@en.
fibo-loan-ln-ev:LegalProceeding a owl:Class;
                                rdfs:isDefinedBy fibo-loan-ln-ev:;
                                rdfs:label "legal proceeding"@en;
                                rdfs:subClassOf fibo-fnd-dt-oc:Occurrence,
                                                [a owl:Restriction ; 
                                                 owl:onProperty fibo-loan-ln-ev:isAgainst ; 
                                                 owl:someValuesFrom cmns-org:LegalPerson],
                                                [a owl:Restriction ; 
                                                 owl:onProperty cmns-rga:isGovernedBy ; 
                                                 owl:someValuesFrom cmns-rga:Jurisdiction];
                                skos:definition "legal step or action taken at the direction of, or by the authority of, a court or agency"@en.
fibo-loan-ln-ev:LoanDefaultProceeding a owl:Class;
                                      rdfs:isDefinedBy fibo-loan-ln-ev:;
                                      rdfs:label "loan default proceeding"@en;
                                      rdfs:subClassOf fibo-loan-ln-ev:LegalProceeding;
                                      skos:definition "[no definition] Further Review This is typically part of mortgagte servicing. THere would typically be a whole department dealing with this. Dealing with default, helping borrowers make payment Collections Default admin Foreclosure Reselling All dealt with by several sub departments. This requires subject matter experts in this area. 1. scoping Identify default as a possible state. This hands off to other business processes. Once you get into the default scenario we are talking about a proces that is going to fall into place over a period of time. The bank works out what to do with the default scenario, e.g. whether it restructures, forecloses, seeks restitution from the security (collateral). It does nto help us to understand the structure of the loan, rather tha consequences of the loan. If we were to further explore the default detail we would bring in other SMEs. And we would have to model a process flow. 1.1 impact on the pool of an MBS Loan Default Proceeding (special ase of legal thing) is an aspect of Default Management / Administratoin. there is also the State of the Loan. Sale / something / fulfilment / fiunduing / approved = servicing mode. Something happens (non payment) =&gt; Default Grace Period followed by negotiation. Some threshold whereby after a given amount of delinquency it needs to go into some other process moving towards foreclosure. Do State Diagram. Stages of loan."@en.
fibo-loan-ln-ev:LoanPaidInFull a owl:Class;
                               rdfs:isDefinedBy fibo-loan-ln-ev:;
                               rdfs:label "loan paid in full"@en;
                               rdfs:subClassOf fibo-loan-ln-ev:LoanPhase.
fibo-loan-ln-ev:LoanPhase a owl:Class;
                          rdfs:isDefinedBy fibo-loan-ln-ev:;
                          rdfs:label "loan phase"@en;
                          rdfs:subClassOf fibo-fnd-arr-lif:LifecycleStage.
fibo-loan-ln-ev:Prepayment a owl:Class;
                           rdfs:isDefinedBy fibo-loan-ln-ev:;
                           rdfs:label "prepayment"@en;
                           rdfs:subClassOf fibo-fnd-pas-psch:Payment.
fibo-loan-ln-ev:RepaymentPhase a owl:Class;
                               rdfs:isDefinedBy fibo-loan-ln-ev:;
                               rdfs:label "repayment phase"@en;
                               rdfs:subClassOf fibo-loan-ln-ev:LoanPhase.
fibo-loan-ln-ev:hasDefaultAmount a owl:ObjectProperty;
                                 rdfs:domain fibo-loan-ln-ev:LoanDefaultProceeding;
                                 rdfs:isDefinedBy fibo-loan-ln-ev:;
                                 rdfs:label "has default amount"@en;
                                 rdfs:range fibo-fnd-acc-cur:MonetaryAmount;
                                 rdfs:subPropertyOf fibo-fnd-acc-cur:hasMonetaryAmount;
                                 skos:definition "amount before the application of sale proceeds and recoveries"@en;
                                 cmns-av:explanatoryNote "Undefined PoC SDM Notes: Default or Foreclosure amount - Total default amount before the application of sale proceeds and recoveries.&nbsp; (AO 146, RR 135) Loan Default Proceeding_cd= (1) Default amount </p> <p> Sale price - Price achieved on sale of property ( AO 147, RR 137 )&nbsp; Loan Default Proceeding_cd= (2) Property Sale </p> <p> Loss on Sale&nbsp;&nbsp; Total loss net of fees, accrued interest etc. after application of sale proceeds (excluding prepayment charge if subordinate to principal recoveries). Show any gain on sale as a negative number (AO 148, RR 138 )&nbsp; Loan Default Proceeding_cd = (3) Loss amount </p> <p> Cumulative Reocveries - ony relevant for cases with losses&nbsp; ( AO 149, RR 139 )&nbsp; Loan Default Proceeding_cd =&nbsp; (4) Recoveries </p> <p> Professional Negligence Recoveries - Any amounts received in settlement or as a result of professional negligence claims against surveyors, solicitors etc. net of any fees / costs ( AO 150, RR 140 )&nbsp; Loan Default Proceeding_cd = (5) Professional Negligence </p><br />"@en.
fibo-loan-ln-ev:hasDisbursementDate a owl:ObjectProperty;
                                    rdfs:domain fibo-loan-ln-ln:Loan;
                                    rdfs:isDefinedBy fibo-loan-ln-ev:;
                                    rdfs:label "disbursement date"@en;
                                    rdfs:range cmns-dt:ExplicitDate;
                                    rdfs:subPropertyOf cmns-dt:hasExplicitDate.
fibo-loan-ln-ev:hasJudgementAmount a owl:ObjectProperty;
                                   rdfs:domain fibo-loan-ln-ev:CourtJudgment;
                                   rdfs:isDefinedBy fibo-loan-ln-ev:;
                                   rdfs:label "has judgement amount"@en;
                                   rdfs:range fibo-fnd-acc-cur:MonetaryAmount;
                                   rdfs:subPropertyOf fibo-fnd-acc-cur:hasMonetaryAmount;
                                   skos:definition "Undefined PoC SDM Notes: Total value of CCJs or equivalent recorded against the primary borrower that were satisfied / unsatsified (at time of underwriting ) is computed by selecting the satisfaction code value of interest and aggregating against all borrower CC Judgements &lt;/p&gt;"@en.
fibo-loan-ln-ev:inDefault a owl:DatatypeProperty;
                          rdfs:domain fibo-loan-ln-ln:Loan;
                          rdfs:isDefinedBy fibo-loan-ln-ev:;
                          rdfs:label "in default"@en;
                          rdfs:range xsd:boolean;
                          skos:definition "Whether the loan is in default."@en.
fibo-loan-ln-ev:isAgainst a owl:ObjectProperty;
                          rdfs:domain fibo-loan-ln-ev:LegalProceeding;
                          rdfs:isDefinedBy fibo-loan-ln-ev:;
                          rdfs:label "is against"@en;
                          rdfs:range cmns-org:LegalPerson.
fibo-loan-ln-ev:isDeferred a owl:DatatypeProperty;
                           rdfs:domain fibo-loan-ln-ln:Loan;
                           rdfs:isDefinedBy fibo-loan-ln-ev:;
                           rdfs:label "is deferred"@en;
                           rdfs:range xsd:boolean.
fibo-loan-ln-ev:isDeliveredBy a owl:ObjectProperty;
                              rdfs:domain fibo-loan-ln-ev:CourtJudgment;
                              rdfs:isDefinedBy fibo-loan-ln-ev:;
                              rdfs:label "is delivered by"@en;
                              rdfs:range fibo-fnd-law-cor:CourtOfLaw.
