                                                 rdfs:isDefinedBy fibo-actus-acv:;
                                                 rdfs:label "ACTUS cashflow category - fixed income";
                                                 skos:definition "cashflow category that applies to basic contracts processed as members of the fixed-income class";
                                                 cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                 cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                 cmns-txt:hasTextValue "Fixed Income".
fibo-actus-acv:ACTUSCashflowCategory-Ownership dcterms:source "https://www.actusfrf.org/taxonomy";
                                               a owl:NamedIndividual,
                                                 fibo-actus-acv:ACTUSCashflowCategory;
                                               rdfs:isDefinedBy fibo-actus-acv:;
                                               rdfs:label "ACTUS cashflow category - ownership";
                                               skos:definition "cashflow category that applies to basic contracts processed as members of the ownership class";
                                               cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                               cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                               cmns-txt:hasTextValue "Ownership".
fibo-actus-acv:ACTUSCashflowCategory-Securitization dcterms:source "https://www.actusfrf.org/taxonomy";
                                                    a owl:NamedIndividual,
                                                      fibo-actus-acv:ACTUSCashflowCategory;
                                                    rdfs:isDefinedBy fibo-actus-acv:;
                                                    rdfs:label "ACTUS cashflow category - securitization";
                                                    skos:definition "cashflow category that applies to combined contracts processed as members of the securitization class";
                                                    cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                    cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                    cmns-txt:hasTextValue "Securitization".
fibo-actus-acv:ACTUSCashflowCategory-Symmetric dcterms:source "https://www.actusfrf.org/taxonomy";
                                               a owl:NamedIndividual,
                                                 fibo-actus-acv:ACTUSCashflowCategory;
                                               rdfs:isDefinedBy fibo-actus-acv:;
                                               rdfs:label "ACTUS cashflow category - symmetric";
                                               skos:definition "cashflow category that applies to combined contracts processed as members of the symmetric class";
                                               cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                               cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                               cmns-txt:hasTextValue "Symmetric".
fibo-actus-acv:ACTUSContractType a owl:Class;
                                 rdfs:isDefinedBy fibo-actus-acv:;
                                 rdfs:label "ACTUS contract type";
                                 rdfs:subClassOf cmns-cls:Classifier,
                                                 [a owl:Restriction ; 
                                                  owl:onProperty fibo-actus-acv:hasCoverageDescription ; 
                                                  owl:allValuesFrom xsd:string],
                                                 [a owl:Restriction ; 
                                                  owl:onProperty cmns-dsg:isDefinedIn ; 
                                                  owl:hasValue fibo-actus-acv:AlgorithmicContractTypesClassificationScheme],
                                                 [a owl:Restriction ; 
                                                  owl:onProperty cmns-cls:classifies ; 
                                                  owl:someValuesFrom fibo-fnd-acc-csf:CashFlowStructure],
                                                 [a owl:Restriction ; 
                                                  owl:onProperty cmns-doc:refersTo ; 
                                                  owl:someValuesFrom [a owl:Class ; 
                                                                      owl:unionOf _:autos18]];
                                 skos:definition "classifier for an algorithmic capability, i.e., the rules, from which the cashflows for a credit agreement, financial instrument or related contract, based on their characteristics, can be analyzed";
                                 cmns-av:explanatoryNote "Cashflows in ACTUS are classified as basic, combined / derivatives, or credit enhancement, and then each of these areas breaks down into a number of subclassifications.";
                                 cmns-txt:hasTextValue "CT".
fibo-actus-acv:ACTUSContractType-Annuity dcterms:source "https://www.actusfrf.org/taxonomy";
                                         a owl:NamedIndividual,
                                           fibo-actus-acv:ACTUSContractType,
                                           [a owl:Restriction ; 
                                            owl:onProperty cmns-doc:refersTo ; 
                                            owl:someValuesFrom fibo-fbc-dae-dbt:CreditAgreementRepaidPeriodically];
                                         rdfs:isDefinedBy fibo-actus-acv:;
                                         rdfs:label "ACTUS contract type - annuity";
                                         skos:definition "cashflow classifier that applies to contracts whose principal is paid fully at the initial exchange date and principal plus interest is repaid periodically in constant amounts until maturity";
                                         skos:isNarrowerThan fibo-actus-acv:ACTUSCashFlowFamily-Basic,
                                                             fibo-actus-acv:ACTUSCashflowCategory-FixedIncome;
                                         skos:note "If the contract has a variable rate, the total amount of principal and interest is recalculated to be fully matured at the maturity date.";
                                         fibo-actus-acv:hasCoverageDescription "classic level payment mortgages, leasing contracts, etc.";
                                         cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                         cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                         cmns-txt:hasTextValue "ANN".
fibo-actus-acv:ACTUSContractType-BondWithWarrant dcterms:source "https://www.actusfrf.org/taxonomy";
                                                 a owl:NamedIndividual,
                                                   fibo-actus-acv:ACTUSContractType,
                                                   [a owl:Restriction ; 
                                                    owl:onProperty cmns-doc:refersTo ; 
                                                    owl:someValuesFrom fibo-sec-dbt-bnd:BondWithWarrant];
                                                 rdfs:isDefinedBy fibo-actus-acv:;
                                                 rdfs:label "ACTUS contract type - bond with warrant";
                                                 skos:definition "cashflow classifier that applies to bonds with attached warrants";
                                                 skos:isNarrowerThan fibo-actus-acv:ACTUSCashFlowFamily-Combined,
                                                                     fibo-actus-acv:ACTUSCashflowCategory-Asymmetric;
                                                 fibo-actus-acv:hasCoverageDescription "Warrants.";
                                                 cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                 cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                 cmns-txt:hasTextValue "BNDWR".
fibo-actus-acv:ACTUSContractType-BoundaryControlledSwitch dcterms:source "https://www.actusfrf.org/taxonomy";
                                                          a owl:NamedIndividual,
                                                            fibo-actus-acv:ACTUSContractType,
                                                            [a owl:Restriction ; 
                                                             owl:onProperty cmns-doc:refersTo ; 
                                                             owl:someValuesFrom fibo-fbc-fi-fi:DerivativeInstrument];
                                                          rdfs:isDefinedBy fibo-actus-acv:;
                                                          rdfs:label "ACTUS contract type - boundary controlled switch";
                                                          skos:definition "cashflow classifier that applies to derivative contracts with subcontract legs which can be activated (knocked in) or extinguished (knocked out) when the underlying asset price reaches a specified value";
                                                          skos:editorialNote "Need to figure out what restrictions are needed on derivative instrument to limit the coverage to the right set of agreements, and then map to those.";
                                                          skos:isNarrowerThan fibo-actus-acv:ACTUSCashFlowFamily-Combined,
                                                                              fibo-actus-acv:ACTUSCashflowCategory-Asymmetric;
                                                          skos:note "The underlying asset may be a stock, index, or exchange-traded fund. Boundary controlled switch contracts with a single boundary are currently defined.";
                                                          fibo-actus-acv:hasCoverageDescription "Knock-in and Knock-out barrier options with a single boundary. Bonus contracts with payout when underlying asset price remains above or below a specified level for specified period.";
                                                          cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                          cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                          cmns-txt:hasTextValue "BCS".
fibo-actus-acv:ACTUSContractType-CallMoney dcterms:source "https://www.actusfrf.org/taxonomy";
                                           a owl:NamedIndividual,
                                             fibo-actus-acv:ACTUSContractType,
                                             [a owl:Restriction ; 
                                              owl:onProperty cmns-doc:refersTo ; 
                                              owl:someValuesFrom [a owl:Class ; 
                                                                  owl:intersectionOf _:autos26]];
                                           rdfs:isDefinedBy fibo-actus-acv:;
                                           rdfs:label "ACTUS contract type - call money";
                                           skos:definition "cashflow classifier that applies to loans that are rolled over as long as they are not called";
                                           skos:editorialNote "Need to figure out whether this only applies to commercial loans and whether additional restrictions are needed to limit coverage to loans whose borrowers and lenders are financial institutions, possibly other things (short term?, interbank only?).";
                                           skos:isNarrowerThan fibo-actus-acv:ACTUSCashFlowFamily-Basic,
                                                               fibo-actus-acv:ACTUSCashflowCategory-FixedIncome;
                                           skos:note "Once called, they must be repaid after the stipulated notice period.";
                                           fibo-actus-acv:hasCoverageDescription "Interbank loans with call features.";
                                           cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                           cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                           cmns-txt:hasTextValue "CLM".
fibo-actus-acv:ACTUSContractType-CapFloorOption dcterms:source "https://www.actusfrf.org/taxonomy";
