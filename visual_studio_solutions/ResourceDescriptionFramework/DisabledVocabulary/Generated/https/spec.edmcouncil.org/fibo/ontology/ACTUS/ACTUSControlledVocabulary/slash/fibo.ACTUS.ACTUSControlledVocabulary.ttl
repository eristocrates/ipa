fibo-actus-acv:ACTUSCashFlowFamily a owl:Class;
                                   rdfs:isDefinedBy fibo-actus-acv:;
                                   rdfs:label "ACTUS cashflow family";
                                   rdfs:subClassOf cmns-cls:Classifier,
                                                   [a owl:Restriction ; 
                                                    owl:onProperty cmns-dsg:isDefinedIn ; 
                                                    owl:hasValue fibo-actus-acv:AlgorithmicContractTypesClassificationScheme],
                                                   [a owl:Restriction ; 
                                                    owl:onProperty cmns-cls:classifies ; 
                                                    owl:someValuesFrom fibo-fnd-acc-csf:CashFlowStructure],
                                                   [a owl:Restriction ; 
                                                    owl:onProperty cmns-doc:refersTo ; 
                                                    owl:someValuesFrom [a owl:Class ; 
                                                                        owl:unionOf _:autos5]];
                                   skos:definition "classifier for the cashflows generated by a group of credit agreements, financial instruments, or related contracts based on their common characteristics from an ACTUS perspective".
fibo-actus-acv:ACTUSCashFlowFamily-Basic dcterms:source "https://www.actusfrf.org/taxonomy";
                                         a owl:NamedIndividual,
                                           fibo-actus-acv:ACTUSCashFlowFamily;
                                         rdfs:isDefinedBy fibo-actus-acv:;
                                         rdfs:label "ACTUS cashflow family - basic";
                                         owl:differentFrom fibo-actus-acv:ACTUSCashFlowFamily-Combined,
                                                           fibo-actus-acv:ACTUSCashFlowFamily-CreditEnhancement;
                                         skos:definition "cashflow family classifier that applies to single (simple) contracts";
                                         cmns-av:explanatoryNote "Basic, or atomic contracts include those individual contracts that have schedules associated with them, i.e., that have traditional maturity dates and future cash flows, or that represent demand deposits, i.e., accounts that have no structured maturity term, whose cash flows or amounts depend on counterparty behavior, as well as other assets such as stocks. Such contracts and related assets are typically bilateral.";
                                         cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                         cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                         cmns-txt:hasTextValue "Basic".
fibo-actus-acv:ACTUSCashFlowFamily-Combined dcterms:source "https://www.actusfrf.org/taxonomy";
                                            a owl:NamedIndividual,
                                              fibo-actus-acv:ACTUSCashFlowFamily;
                                            rdfs:isDefinedBy fibo-actus-acv:;
                                            rdfs:label "ACTUS cashflow family - combined";
                                            owl:differentFrom fibo-actus-acv:ACTUSCashFlowFamily-Basic,
                                                              fibo-actus-acv:ACTUSCashFlowFamily-CreditEnhancement;
                                            skos:definition "cashflow family classifier that applies to combined (complex, derived) contracts, such as derivatives";
                                            cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                            cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                            cmns-txt:hasTextValue "Combined".
fibo-actus-acv:ACTUSCashFlowFamily-CreditEnhancement dcterms:source "https://www.actusfrf.org/taxonomy";
                                                     a owl:NamedIndividual,
                                                       fibo-actus-acv:ACTUSCashFlowFamily;
                                                     rdfs:isDefinedBy fibo-actus-acv:;
                                                     rdfs:label "ACTUS cashflow family - credit enhancement";
                                                     owl:differentFrom fibo-actus-acv:ACTUSCashFlowFamily-Basic,
                                                                       fibo-actus-acv:ACTUSCashFlowFamily-Combined;
                                                     skos:definition "cashflow family classifier that applies to agreements that are related to other agreements or accounts, and provide credit support to existing contracts";
                                                     cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                     cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                     cmns-txt:hasTextValue "Credit Enhancement".
fibo-actus-acv:ACTUSCashflowCategory a owl:Class;
                                     rdfs:isDefinedBy fibo-actus-acv:;
                                     rdfs:label "ACTUS cashflow category";
                                     rdfs:subClassOf cmns-cls:Classifier,
                                                     [a owl:Restriction ; 
                                                      owl:onProperty cmns-dsg:isDefinedIn ; 
                                                      owl:hasValue fibo-actus-acv:AlgorithmicContractTypesClassificationScheme],
                                                     [a owl:Restriction ; 
                                                      owl:onProperty cmns-cls:classifies ; 
                                                      owl:someValuesFrom fibo-fnd-acc-csf:CashFlowStructure],
                                                     [a owl:Restriction ; 
                                                      owl:onProperty cmns-doc:refersTo ; 
                                                      owl:someValuesFrom [a owl:Class ; 
                                                                          owl:unionOf _:autos11]];
                                     skos:definition "classifier for the cashflows generated by a group of credit agreements, financial instruments, or related contracts, within an ACTUS cashflow family, based on their more specific, common characteristics".
fibo-actus-acv:ACTUSCashflowCategory-Asymmetric dcterms:source "https://www.actusfrf.org/taxonomy";
