                                                cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                cmns-txt:hasTextValue "Asymmetric".
fibo-actus-acv:ACTUSCashflowCategory-CreditEnhancement dcterms:source "https://www.actusfrf.org/taxonomy";
                                                       a owl:NamedIndividual,
                                                         fibo-actus-acv:ACTUSCashflowCategory;
                                                       rdfs:isDefinedBy fibo-actus-acv:;
                                                       rdfs:label "ACTUS cashflow category - credit enhancement";
                                                       skos:definition "cashflow category that applies to basic contracts processed as members of the credit enhancement class";
                                                       cmns-col:isMemberOf fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                       cmns-dsg:isDefinedIn fibo-actus-acv:AlgorithmicContractTypesClassificationScheme;
                                                       cmns-txt:hasTextValue "Credit Enhancement".
fibo-actus-acv:ACTUSCashflowCategory-FixedIncome dcterms:source "https://www.actusfrf.org/taxonomy";
