                  a owl:Ontology;
                  rdfs:label "Credit Ratings Ontology"@en;
                  owl:imports <https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/FinancialInstruments/FinancialInstruments/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/ProductsAndServices/ClientsAndAccounts/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/ProductsAndServices/FinancialProductsAndServices/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/Ratings/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/Reporting/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/DatesAndTimes/FinancialDates/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/ProductsAndServices/ProductsAndServices/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Relations/Relations/>,
                              <https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/AnnotationVocabulary/>,
                              cmns-av:,
                              cmns-cls:,
                              cmns-col:,
                              cmns-dt:,
                              cmns-dsg:,
                              cmns-doc:,
                              cmns-id:,
                              cmns-org:,
                              cmns-pts:,
                              cmns-qtu:,
                              cmns-rlcmp:;
                  owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/DebtAndEquities/CreditRatings/>;
                  fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Provisional;
                  cmns-av:copyright "Copyright (c) 2018-2025 EDM Council, Inc.",
                                    "Copyright (c) 2018-2025 Object Management Group, Inc.".
fibo-fbc-dae-crt:AlertCreditMessage a owl:NamedIndividual,
                                      fibo-fbc-dae-crt:CreditMessageType;
                                    rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                    rdfs:label "alert credit message".
fibo-fbc-dae-crt:AlternativeAQuality a owl:NamedIndividual,
                                       fibo-fbc-dae-crt:CreditQuality;
                                     rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                     rdfs:label "alternative-A quality";
                                     skos:definition "classifier for mortgages that are considered to be riskier than prime quality but less risky than subprime quality";
                                     cmns-av:abbreviation "Alt-A";
                                     cmns-av:explanatoryNote "Typically Alt-A mortgages are characterized by borrowers with less than full documentation, average credit scores, higher loan-to-values, and more investment properties and secondary homes.";
                                     cmns-av:synonym "Alternative A-paper".
fibo-fbc-dae-crt:CreditInquiry a owl:Class;
                               rdfs:isDefinedBy fibo-fbc-dae-crt:;
                               rdfs:label "credit inquiry";
                               rdfs:subClassOf fibo-fnd-arr-rep:RequestActivity,
                                               [a owl:Restriction ; 
                                                owl:onProperty fibo-fbc-dae-crt:concernsParty ; 
                                                owl:someValuesFrom cmns-pts:Party],
                                               [a owl:Restriction ; 
                                                owl:onProperty fibo-fnd-arr-rep:isRequestedOf ; 
                                                owl:someValuesFrom fibo-fbc-dae-crt:CreditRatingAgency],
                                               [a owl:Restriction ; 
                                                owl:onProperty cmns-cls:isClassifiedBy ; 
                                                owl:someValuesFrom fibo-fbc-dae-crt:CreditInquiryType];
                               skos:definition "request from a lender to a credit repository to obtain information regarding a prospective borrower's creditworthiness".
fibo-fbc-dae-crt:CreditInquiryType a owl:Class;
                                   rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                   rdfs:label "credit inquiry type";
                                   rdfs:subClassOf cmns-cls:Classifier;
                                   skos:definition "classifier indicating whether a credit inquiry is a result of a borrower's direct authorization or by some indirect means".
fibo-fbc-dae-crt:CreditMessage a owl:Class;
                               rdfs:isDefinedBy fibo-fbc-dae-crt:;
                               rdfs:label "credit message";
                               rdfs:subClassOf cmns-doc:Notice,
                                               [a owl:Restriction ; 
                                                owl:onProperty cmns-cls:isClassifiedBy ; 
                                                owl:someValuesFrom fibo-fbc-dae-crt:CreditMessageType];
                               skos:definition "notice on a credit report to alert a reviewer of special or exceptional information".
fibo-fbc-dae-crt:CreditMessageType a owl:Class;
                                   rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                   rdfs:label "credit message type";
                                   rdfs:subClassOf cmns-cls:Classifier;
                                   skos:definition "classifier that categorizes credit messages".
fibo-fbc-dae-crt:CreditQuality a owl:Class;
                               rdfs:isDefinedBy fibo-fbc-dae-crt:;
                               rdfs:label "credit quality";
                               rdfs:subClassOf fibo-fnd-arr-rt:RatingScore;
                               skos:definition "classifier for borrowers, rates, or holdings in the lending market with respect to the quality of the credit".
fibo-fbc-dae-crt:CreditRating a owl:Class;
                              rdfs:isDefinedBy fibo-fbc-dae-crt:;
                              rdfs:label "credit rating"@en;
                              rdfs:subClassOf fibo-fnd-arr-rt:Rating,
                                              [a owl:Restriction ; 
                                               owl:onProperty cmns-qtu:isDerivedFrom ; 
                                               owl:onClass fibo-fbc-dae-crt:CreditRatingModel ; 
                                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                              [a owl:Restriction ; 
                                               owl:onProperty fibo-fnd-rel-rel:isIssuedBy ; 
                                               owl:someValuesFrom fibo-fbc-dae-crt:CreditRatingAgency];
                              skos:definition "assessment of creditworthiness of a borrower generally or with respect to a particular debt or financial obligation"@en;
                              cmns-av:explanatoryNote "Typically, a credit rating is provided as a detailed report based on the financial history of borrowing or lending and creditworthiness of the entity or person derived from income statements, historical records related to borrowing, etc. with an aim to determine their ability to meet debt obligations."@en.
fibo-fbc-dae-crt:CreditRatingAgency a owl:Class;
                                    rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                    rdfs:label "credit rating agency"@en;
                                    rdfs:subClassOf fibo-fbc-pas-fpas:FinancialServiceProvider,
                                                    fibo-fnd-arr-rt:RatingAgency,
                                                    [a owl:Restriction ; 
                                                     owl:onProperty fibo-fnd-rel-rel:issues ; 
                                                     owl:someValuesFrom fibo-fbc-dae-crt:CreditRating],
                                                    [a owl:Restriction ; 
                                                     owl:onProperty cmns-org:manages ; 
                                                     owl:someValuesFrom fibo-fbc-dae-crt:CreditRatingScale];
                                    skos:definition "rating agency that publishes reports assessing the creditworthiness of a borrower or legal entity, either generally or with respect to a specific obligation"@en.
fibo-fbc-dae-crt:CreditRatingModel a owl:Class;
                                   rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                   rdfs:label "credit rating model";
                                   rdfs:subClassOf cmns-qtu:Expression,
                                                   [a owl:Restriction ; 
                                                    owl:onProperty fibo-fnd-rel-rel:produces ; 
                                                    owl:someValuesFrom fibo-fnd-arr-rt:RatingScore],
                                                   [a owl:Restriction ; 
                                                    owl:onProperty cmns-cls:isClassifiedBy ; 
                                                    owl:someValuesFrom fibo-fbc-dae-crt:CreditRatingModelType],
                                                   [a owl:Restriction ; 
                                                    owl:onProperty cmns-dsg:hasTextualName ; 
                                                    owl:someValuesFrom xsd:string];
                                   skos:definition "algorithm for computing a credit rating";
                                   cmns-av:usageNote "Use dct:hasVersion to specify a version for the credit score model.".
fibo-fbc-dae-crt:CreditRatingModelType a owl:Class;
                                       rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                       rdfs:label "credit score model type";
                                       rdfs:subClassOf cmns-cls:Classifier;
                                       skos:definition "a type corresonding to a family of credit scoring algorithms sharing common characteristics".
fibo-fbc-dae-crt:CreditRatingScale a owl:Class;
                                   rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                   rdfs:label "credit rating scale"@en;
                                   rdfs:subClassOf fibo-fnd-arr-rt:RatingScale,
                                                   [a owl:Restriction ; 
                                                    owl:onProperty cmns-org:isManagedBy ; 
                                                    owl:someValuesFrom fibo-fbc-dae-crt:CreditRatingAgency];
                                   skos:definition "rating scale for assigning a credit rating to individual, entity, or instrument, including defining a set of codes or numeric scores and indicating how an assessment that is assigned a score based on that scale means"@en.
fibo-fbc-dae-crt:CreditReport a owl:Class;
                              rdfs:isDefinedBy fibo-fbc-dae-crt:;
                              rdfs:label "credit report";
                              rdfs:subClassOf fibo-fnd-arr-rt:RatingReport,
                                              [a owl:Restriction ; 
                                               owl:onProperty cmns-col:comprises ; 
                                               owl:onClass fibo-fbc-dae-crt:CreditInquiry ; 
                                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                              [a owl:Restriction ; 
                                               owl:onProperty cmns-col:comprises ; 
                                               owl:onClass fibo-fbc-dae-crt:CreditMessage ; 
                                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                              [a owl:Restriction ; 
                                               owl:onProperty cmns-col:comprises ; 
                                               owl:onClass fibo-fbc-dae-crt:CreditRating ; 
                                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                              [a owl:Restriction ; 
                                               owl:onProperty fibo-fnd-rel-rel:isProducedBy ; 
                                               owl:onClass fibo-fbc-dae-crt:CreditRatingAgency ; 
                                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                              [a owl:Restriction ; 
                                               owl:onProperty cmns-cls:isClassifiedBy ; 
                                               owl:onClass fibo-fbc-dae-crt:CreditReportCategory ; 
                                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                              [a owl:Restriction ; 
                                               owl:onProperty fibo-fnd-rel-rel:exemplifies ; 
                                               owl:onClass fibo-fbc-dae-crt:CreditReportProduct ; 
                                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                              [a owl:Restriction ; 
                                               owl:onProperty cmns-col:comprises ; 
                                               owl:onClass fibo-fbc-dae-crt:CreditTradeline ; 
                                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                              [a owl:Restriction ; 
                                               owl:onProperty cmns-id:isIdentifiedBy ; 
                                               owl:onClass cmns-id:Identifier ; 
                                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                              [a owl:Restriction ; 
                                               owl:onProperty cmns-doc:hasDataSource ; 
                                               owl:onClass [a owl:Restriction ; 
                                                            owl:onProperty cmns-rlcmp:playsRole ; 
                                                            owl:someValuesFrom fibo-fbc-dae-crt:CreditRatingAgency] ; 
                                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger],
                                              [a owl:Restriction ; 
                                               owl:onProperty fibo-fbc-dae-crt:concernsParty ; 
                                               owl:someValuesFrom cmns-pts:Party],
                                              [a owl:Restriction ; 
                                               owl:onProperty fibo-fbc-dae-crt:hasCoveragePeriod ; 
                                               owl:someValuesFrom cmns-dt:DatePeriod],
                                              [a owl:Restriction ; 
                                               owl:onProperty fibo-fnd-dt-fd:hasAsOfDate ; 
                                               owl:someValuesFrom cmns-dt:Date];
                              skos:definition "report describing the creditworthiness and related credit attributes of a borrower";
                              cmns-av:explanatoryNote "This is typically provided by a credit rating agency but could be produced by an internal proprietary model as well.".
fibo-fbc-dae-crt:CreditReportCategory a owl:Class;
                                      rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                      rdfs:label "credit report category";
                                      rdfs:subClassOf cmns-cls:Classifier;
                                      skos:definition "classifier for credit reports, often available from multiple vendors".
fibo-fbc-dae-crt:CreditReportProduct a owl:Class;
                                     rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                     rdfs:label "credit report product";
                                     rdfs:subClassOf fibo-fbc-pas-fpas:FinancialProduct,
                                                     fibo-fnd-pas-pas:OffTheShelfProduct,
                                                     [a owl:Restriction ; 
                                                      owl:onProperty fibo-fnd-rel-rel:isProducedBy ; 
                                                      owl:someValuesFrom fibo-fbc-dae-crt:CreditRatingAgency];
                                     skos:definition "branded credit report offered in the marketplace".
fibo-fbc-dae-crt:CreditTradeline a owl:Class;
                                 rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                 rdfs:label "credit tradeline";
                                 rdfs:subClassOf fibo-fnd-arr-rep:Report,
                                                 [a owl:Restriction ; 
                                                  owl:onProperty cmns-col:isIncludedIn ; 
                                                  owl:someValuesFrom fibo-fbc-dae-crt:CreditReport],
                                                 [a owl:Restriction ; 
                                                  owl:onProperty cmns-qtu:isDerivedFrom ; 
                                                  owl:someValuesFrom fibo-fbc-pas-caa:Account];
                                 skos:definition "report derived from the transaction history of a credit account";
                                 cmns-av:explanatoryNote "A tradeline on a credit report refers to a specific credit account. Tradelines report snapshot details derived from a combination of account features and payment history, and are used by credit reporting agencies as inputs to the analysis process that determines a party's credit rating.".
fibo-fbc-dae-crt:CreditWatchDirection a owl:Class;
                                      rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                      rdfs:label "credit watch direction"@en;
                                      rdfs:subClassOf cmns-cls:Classifier.
fibo-fbc-dae-crt:CreditWatchDowngrade a owl:NamedIndividual,
                                        fibo-fbc-dae-crt:CreditWatchDirection;
                                      rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                      rdfs:label "credit watch downgrade"@en;
                                      skos:example "On April 22, 2022, OppFi (NYSE:OPFI) was downgraded by Zacks Investment Research from a 'hold' rating to a 'sell' rating in a research report issued to clients and investors on Friday, Zacks.com reports."@en.
fibo-fbc-dae-crt:CreditWatchOutlook a owl:Class;
                                    rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                    rdfs:label "credit watch outlook"@en;
                                    rdfs:subClassOf fibo-fnd-arr-rt:Rating.
fibo-fbc-dae-crt:CreditWatchUpgrade a owl:NamedIndividual,
                                      fibo-fbc-dae-crt:CreditWatchDirection;
                                    rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                    rdfs:label "credit watch upgrade"@en.
fibo-fbc-dae-crt:FreezeCreditMessage a owl:NamedIndividual,
                                       fibo-fbc-dae-crt:CreditMessageType;
                                     rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                     rdfs:label "freeze credit message".
fibo-fbc-dae-crt:HardCreditInquiry a owl:NamedIndividual,
                                     fibo-fbc-dae-crt:CreditInquiryType;
                                   rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                   rdfs:label "hard credit inquiry".
fibo-fbc-dae-crt:InstrumentCreditRating a owl:Class;
                                        rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                        rdfs:label "instrument credit rating"@en;
                                        rdfs:subClassOf fibo-fbc-dae-crt:InvestmentCreditRating,
                                                        [a owl:Restriction ; 
                                                         owl:onProperty fibo-fnd-arr-rt:rates ; 
                                                         owl:someValuesFrom fibo-fbc-fi-fi:FinancialInstrument];
                                        skos:definition "investment credit rating that provides an opinion of creditworthiness of an instrument, typically with some relationship to the creditworthiness of the issuer(s)"@en.
fibo-fbc-dae-crt:InvestmentCreditRating a owl:Class;
                                        rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                        rdfs:label "investment credit rating"@en;
                                        rdfs:subClassOf fibo-fbc-dae-crt:CreditRating;
                                        skos:definition "credit rating that provides an opinion of creditworthiness of an investment, including but not limited to an instrument or organization"@en.
fibo-fbc-dae-crt:InvestmentGradeQuality a owl:NamedIndividual,
                                          fibo-fbc-dae-crt:CreditQuality;
                                        rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                        rdfs:label "investment grade quality";
                                        skos:definition "classifier for debt instruments that are considered to be of high quality and have a low risk of default";
                                        cmns-av:explanatoryNote "Bond rating firms use different designations, often consisting of the upper- and lower-case letters to identify a bond's credit quality rating. 'AAA' and 'AA' (high credit quality) and 'A' and 'BBB' (medium credit quality) are considered investment grade. Credit ratings for bonds below these designations ('BB,' 'B,' 'CCC,' etc.) are considered low credit quality, and are commonly referred to as 'junk bonds.'".
fibo-fbc-dae-crt:NonprimeQuality a owl:NamedIndividual,
                                   fibo-fbc-dae-crt:CreditQuality;
                                 rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                 rdfs:label "nonprime quality";
                                 skos:definition "classifier for borrowers, rates, or holdings that have poor quality, such as borrowers with poor credit history due to a short sale, bankruptcy, foreclosure and/or other negative credit events within the last several years";
                                 cmns-av:explanatoryNote "Nonprime loans may be offered to individuals who are unable to qualify for a conventional loan.".
fibo-fbc-dae-crt:OnWatchOutlook a owl:NamedIndividual,
                                  fibo-fbc-dae-crt:CreditWatchOutlook;
                                rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                rdfs:label "on watch outlook"@en.
fibo-fbc-dae-crt:OrganizationCreditRating a owl:Class;
                                          rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                          rdfs:label "organization credit rating"@en;
                                          rdfs:subClassOf fibo-fbc-dae-crt:CreditRating,
                                                          [a owl:Restriction ; 
                                                           owl:onProperty fibo-fnd-arr-rt:rates ; 
                                                           owl:someValuesFrom cmns-org:FormalOrganization];
                                          skos:definition "credit rating that provides an opinion of creditworthiness of an organization"@en.
fibo-fbc-dae-crt:PrimeQuality a owl:NamedIndividual,
                                fibo-fbc-dae-crt:CreditQuality;
                              rdfs:isDefinedBy fibo-fbc-dae-crt:;
                              rdfs:label "prime quality";
                              skos:definition "classifier for borrowers, rates, or holdings that are considered to be of high quality";
                              cmns-av:explanatoryNote "This classification often refers to loans made to high-quality borrowers that are offered prime or relatively low interest rates. Prime loans have low default risk.";
                              cmns-av:synonym "A-paper".
fibo-fbc-dae-crt:ShortTermCreditRating a owl:Class;
                                       rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                       rdfs:label "short term credit rating"@en;
                                       rdfs:subClassOf fibo-fbc-dae-crt:InvestmentCreditRating;
                                       skos:definition "credit rating that provides an opinion of the probability of an individual or organization going into default within a year"@en.
fibo-fbc-dae-crt:SoftCreditInquiry a owl:NamedIndividual,
                                     fibo-fbc-dae-crt:CreditInquiryType;
                                   rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                   rdfs:label "soft credit inquiry".
fibo-fbc-dae-crt:StableOutlook a owl:NamedIndividual,
                                 fibo-fbc-dae-crt:CreditWatchOutlook;
                               rdfs:isDefinedBy fibo-fbc-dae-crt:;
                               rdfs:label "stable outlook"@en.
fibo-fbc-dae-crt:SubprimeQuality a owl:NamedIndividual,
                                   fibo-fbc-dae-crt:CreditQuality;
                                 rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                 rdfs:label "subprime quality";
                                 skos:definition "classifier for borrowers, rates, or holdings that are considered below-average quality, such as borrowers with a tarnished or limited credit history, and may be subject to higher than average interest rates";
                                 cmns-av:explanatoryNote "Subprime refers to borrowers or loans, usually offered at rates well above the prime rate. Subprime lending is higher risk, given the lower credit rating of borrowers.".
fibo-fbc-dae-crt:concernsParty a owl:ObjectProperty;
                               rdfs:isDefinedBy fibo-fbc-dae-crt:;
                               rdfs:label "concerns party";
                               rdfs:subPropertyOf cmns-pts:hasParty;
                               skos:definition "indicates the party for which something is being or has been requested".
fibo-fbc-dae-crt:hasCoveragePeriod a owl:ObjectProperty;
                                   rdfs:isDefinedBy fibo-fbc-dae-crt:;
                                   rdfs:label "has coverage period";
                                   rdfs:range cmns-dt:DatePeriod;
                                   rdfs:subPropertyOf cmns-dt:hasDatePeriod;
